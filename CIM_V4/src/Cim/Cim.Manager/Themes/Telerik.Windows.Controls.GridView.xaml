    <ControlTemplate x:Key="GridViewEditorPresenterValidationToolTipTemplate" TargetType="ToolTip">
        <Grid
            x:Name="Root"
            Margin="2,0,0,0"
            Background="Transparent"
            Opacity="0"
            RenderTransformOrigin="0 0">
            <Grid.RenderTransform>
                <TranslateTransform x:Name="xform" X="-25" />
            </Grid.RenderTransform>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Border
                Grid.Column="1"
                MinHeight="20"
                Padding="4,3,4,3"
                Background="{telerik1:GreenResource ResourceKey=ValidationBrush}">
                <TextBlock
                    MaxWidth="250"
                    VerticalAlignment="Center"
                    FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                    FontSize="{telerik1:GreenResource ResourceKey=FontSize}"
                    Foreground="{telerik1:GreenResource ResourceKey=FixedBrush}"
                    Text="{TemplateBinding Content}"
                    TextWrapping="Wrap" />
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsOpen" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard x:Name="OpenAnimation">
                        <Storyboard>
                            <DoubleAnimation
                                Storyboard.TargetName="xform"
                                Storyboard.TargetProperty="X"
                                To="0"
                                Duration="0:0:0.2" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard x:Name="CloseAnimation">
                        <Storyboard>
                            <DoubleAnimation
                                Storyboard.TargetName="xform"
                                Storyboard.TargetProperty="X"
                                To="-25"
                                Duration="0:0:0.2" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
                <Setter TargetName="Root" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsOpen" Value="False">
                <Setter TargetName="Root" Property="Opacity" Value="0" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
