        <Setter Property="Template" Value="{StaticResource RadGridViewTemplate}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="GroupPanelBackground" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="GroupPanelForeground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="SearchPanelBackground" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="SearchPanelForeground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="VerticalGridLinesBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="HorizontalGridLinesBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="AlternateRowBackground" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}" />
        <Setter Property="RowHeight" Value="26" />
        <Setter Property="Tag" Value="Green" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
    </Style>
    <Style BasedOn="{StaticResource RadGridViewStyle}" TargetType="controls:RadGridView" />
    <Style x:Key="TreeListViewDragCueStyle" TargetType="controls:TreeListViewDragCue">
        <Setter Property="IsHitTestVisible" Value="False" />
        <Setter Property="Padding" Value="4,2" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:TreeListViewDragCue">
                    <StackPanel>
                        <Grid
                            MinHeight="22"
                            HorizontalAlignment="Left"
                            Visibility="{TemplateBinding DragPreviewVisibility}">
                            <Grid.Resources>
                                <Style TargetType="TextBlock">
                                    <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                                </Style>
                            </Grid.Resources>
                            <Border
                                Background="{telerik1:GreenResource ResourceKey=AccentHighBrush}"
                                BorderBrush="{telerik1:GreenResource ResourceKey=AccentHighBrush}"
                                BorderThickness="1"
                                Opacity="0.9">
                                <ItemsPresenter Margin="{TemplateBinding Padding}" />
                            </Border>
                        </Grid>
                        <Border
                            MinHeight="22"
                            Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                            BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                            BorderThickness="1"
                            Opacity="0.9"
                            Visibility="{TemplateBinding DragTooltipVisibility}">
                            <Border.Effect>
                                <DropShadowEffect
                                    BlurRadius="3"
                                    Opacity="0.1"
                                    ShadowDepth="3" />
                            </Border.Effect>
                            <StackPanel Margin="3,0,0,0" Orientation="Horizontal">
                                <ContentPresenter
                                    x:Name="DropImpossibleIconElement"
                                    Content="{TemplateBinding DropImpossibleIcon}"
                                    ContentTemplate="{TemplateBinding DropImpossibleIconTemplate}"
                                    Visibility="Collapsed" />
                                <ContentPresenter
                                    x:Name="DropPossibleIconElement"
                                    Content="{TemplateBinding DropPossibleIcon}"
                                    ContentTemplate="{TemplateBinding DropPossibleIconTemplate}" />
                                <ContentControl
                                    Margin="4,0,0,0"
                                    VerticalAlignment="Center"
                                    Content="{TemplateBinding DragActionContent}"
                                    ContentTemplate="{TemplateBinding DragActionContentTemplate}"
                                    FontFamily="{TemplateBinding FontFamily}"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
                                <ContentControl
                                    VerticalAlignment="Center"
                                    Content="{TemplateBinding DragTooltipContent}"
                                    ContentTemplate="{TemplateBinding DragTooltipContentTemplate}"
                                    FontFamily="{TemplateBinding FontFamily}"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                            </StackPanel>
                        </Border>
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDropPossible" Value="False">
                            <Setter TargetName="DropImpossibleIconElement" Property="Visibility" Value="Visible" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropImpossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid UseLayoutRounding="True">
                        <Path
                            Width="12"
                            Height="12"
                            VerticalAlignment="Center"
                            Data="{StaticResource DropImpossibleIcon}"
                            Fill="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                            SnapsToDevicePixels="True"
                            Stretch="Fill" />
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropPossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid UseLayoutRounding="True">
                        <Path
                            Width="8"
                            Height="8"
                            VerticalAlignment="Center"
                            Data="{StaticResource PointerRight8x8}"
                            Fill="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                            SnapsToDevicePixels="True"
                            Stretch="Fill" />
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="SnapsToDevicePixels" Value="True" />
    </Style>
    <Style BasedOn="{StaticResource TreeListViewDragCueStyle}" TargetType="controls:TreeListViewDragCue" />
    <ControlTemplate x:Key="RadTreeListViewTemplate" TargetType="controls:RadTreeListView">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="0" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Border
                x:Name="PART_MasterGridContainer"
                Grid.Row="2"
                Grid.ColumnSpan="3"
                Margin="0,-1,0,0"
                Padding="{TemplateBinding Padding}"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}" />
            <ItemsControl
                Name="PART_ControlPanelItemsControl"
                Grid.Row="1"
                Grid.Column="2"
                HorizontalAlignment="Right"
                ItemsSource="{TemplateBinding ControlPanelItems}">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Horizontal" />
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <grid:ControlPanelItemControl />
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
            <grid:GridViewScrollViewer
                x:Name="PART_ItemsScrollViewer"
                Grid.Row="2"
                Grid.RowSpan="2"
                Grid.ColumnSpan="3"
                Margin="1,0,1,1"
                CanContentScroll="True">
                <grid:GridViewScrollViewer.HeaderRow>
                    <grid:GridViewHeaderRow x:Name="PART_HeaderRow" IndentLevel="{TemplateBinding GroupCount}" />
                </grid:GridViewScrollViewer.HeaderRow>
                <grid:GridViewScrollViewer.NewRow>
                    <grid:GridViewNewRow Name="PART_AddNewRow" IndentLevel="{TemplateBinding GroupCount}" />
                </grid:GridViewScrollViewer.NewRow>
                <grid:GridViewScrollViewer.FooterRow>
                    <grid:GridViewFooterRow x:Name="PART_FooterRow" IndentLevel="{TemplateBinding GroupCount}" />
                </grid:GridViewScrollViewer.FooterRow>
                <grid:GridViewVirtualizingPanel x:Name="PART_GridViewVirtualizingPanel" />
            </grid:GridViewScrollViewer>
            <grid:ScrollPositionIndicator
                x:Name="PART_ScrollPositionIndicator"
                Grid.Row="2"
                Grid.ColumnSpan="3"
                Margin="0,0,28,0"
                HorizontalAlignment="Right"
                ContentTemplate="{TemplateBinding ScrollPositionIndicatorTemplate}"
                IsHitTestVisible="False"
                Visibility="{Binding IsScrolling, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
            <Border
                x:Name="PART_FrozenColumnsPreview"
                Grid.Row="2"
                Grid.RowSpan="3"
                Width="6"
                HorizontalAlignment="Left"
                VerticalAlignment="Stretch"
                Background="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                Opacity="0.1"
                Visibility="Collapsed">
                <Border
                    Width="5"
                    HorizontalAlignment="Right"
                    BorderBrush="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                    BorderThickness="1,0,0,0" />
            </Border>
            <Border
                x:Name="PART_FrozenColumnsPreviewRight"
                Grid.Row="2"
                Grid.RowSpan="3"
                Width="6"
                HorizontalAlignment="Left"
                VerticalAlignment="Stretch"
                Background="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                Opacity="0.1"
                Visibility="Collapsed">
                <Border
                    Width="5"
                    HorizontalAlignment="Right"
                    BorderBrush="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                    BorderThickness="1,0,0,0" />
            </Border>
            <grid:GridViewLoadingIndicator
                x:Name="PART_GridViewLoadingIndicator"
                Grid.RowSpan="4"
                Grid.ColumnSpan="3"
                BusyContent="{Binding BusyIndicatorContent, RelativeSource={RelativeSource TemplatedParent}}"
                BusyContentTemplate="{Binding BusyIndicatorContentTemplate, RelativeSource={RelativeSource TemplatedParent}}"
                IsBusy="{TemplateBinding IsBusy}"
                IsIndeterminate="{Binding BusyIndicatorIsIndeterminate, RelativeSource={RelativeSource TemplatedParent}}"
                ProgressValue="{Binding BusyIndicatorProgressValue, RelativeSource={RelativeSource TemplatedParent}}"
                Visibility="Collapsed" />
            <Grid
                x:Name="DragBetweenItemsFeedback"
                Grid.Row="2"
                Grid.ColumnSpan="3"
                Height="8"
                Margin="{TemplateBinding Padding}"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                IsHitTestVisible="False"
                Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="8" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Ellipse
                    Width="8"
                    Height="8"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Stroke="{telerik1:GreenResource ResourceKey=AccentHighBrush}"
                    StrokeThickness="2" />
                <Rectangle
                    Grid.Column="1"
                    Height="2"
                    Margin="-2,3,0,0"
                    VerticalAlignment="Top"
                    Fill="{telerik1:GreenResource ResourceKey=AccentHighBrush}"
                    RadiusX="2"
                    RadiusY="2" />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsBusy" Value="True">
                <Setter TargetName="PART_GridViewLoadingIndicator" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="ShowColumnFooters" Value="false">
                <Setter TargetName="PART_FooterRow" Property="Visibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="ShowColumnHeaders" Value="false">
                <Setter TargetName="PART_HeaderRow" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadTreeListViewStyle" TargetType="controls:RadTreeListView">
        <Setter Property="Template" Value="{StaticResource RadTreeListViewTemplate}" />
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="VerticalGridLinesBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="HorizontalGridLinesBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="AlternateRowBackground" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}" />
        <Setter Property="RowHeight" Value="26" />
        <Setter Property="Tag" Value="Green" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="TreeLinesBrush" Value="{telerik1:GreenResource ResourceKey=StrongBrush}" />
        <Setter Property="TreeLinesStrokeThickness" Value="1" />
    </Style>
    <Style BasedOn="{StaticResource RadTreeListViewStyle}" TargetType="controls:RadTreeListView" />
    <ControlTemplate x:Key="TreeListViewRowTemplate" TargetType="treeList:TreeListViewRow">
        <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <grid:SelectiveScrollingGrid x:Name="grid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Border
                    x:Name="Background_Main"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="{TemplateBinding Margin}"
                    Padding="{TemplateBinding Padding}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    Background="{TemplateBinding Background}" />
                <grid:DetailsPresenter
                    x:Name="PART_DetailsPresenter"
                    Grid.Row="1"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="0,-1,0,0"
                    BorderThickness="0"
                    DetailsProvider="{TemplateBinding DetailsProvider}" />
                <Border
                    x:Name="Background_Invalid"
                    Grid.Column="1"
                    Grid.ColumnSpan="4"
                    Background="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                    Opacity="0.3"
                    Visibility="Collapsed" />
                <Border
                    Grid.RowSpan="3"
                    Width="26"
                    grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                    Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                    Visibility="{TemplateBinding RowIndicatorVisibility}" />
                <Border
                    x:Name="PART_RowBorder"
                    Grid.RowSpan="3"
                    Grid.ColumnSpan="4"
                    BorderBrush="{TemplateBinding HorizontalGridLinesBrush}"
                    BorderThickness="{Binding HorizontalGridLinesWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource GridLineWidthToThicknessConverter}, ConverterParameter=Bottom}" />
                <Border
                    x:Name="Background_Over"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,1"
                    Background="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    Visibility="Collapsed" />
                <Border
                    x:Name="Background_Selected_Unfocused"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,1"
                    Background="{telerik1:GreenResource ResourceKey=BasicBrush}"
                    Visibility="Collapsed" />
                <Border
                    x:Name="Background_Selected"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,1"
                    Background="{telerik1:GreenResource ResourceKey=AccentHighBrush}"
                    Visibility="Collapsed" />
                <controls:GridViewToggleButton
                    x:Name="PART_ExpandButton"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    Width="26"
                    HorizontalAlignment="Left"
                    HorizontalContentAlignment="Center"
                    IsChecked="{Binding IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                    IsTabStop="False">
                    <controls:GridViewToggleButton.Style>
                        <Style BasedOn="{StaticResource GridViewToggleButtonStyle}" TargetType="controls:GridViewToggleButton">
                            <Setter Property="PresentationMode" Value="Arrow" />
                        </Style>
                    </controls:GridViewToggleButton.Style>
                </controls:GridViewToggleButton>
                <Border
                    x:Name="PART_IndicatorPresenter"
                    Grid.RowSpan="3"
                    Width="26"
                    VerticalAlignment="Stretch"
                    grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                    BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                    BorderThickness="0,0,1,0"
                    Visibility="{TemplateBinding RowIndicatorVisibility}">
                    <Grid>
                        <Grid
                            x:Name="NavigatorIndicator"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Visibility="Collapsed">
                            <Path
                                Width="5"
                                Height="8"
                                Margin="1,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Data="{StaticResource ThinArrowRight5x8}"
                                Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                                Stretch="Fill" />
                        </Grid>
                        <Grid
                            x:Name="SelectedNavigatorIndicator"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Visibility="Collapsed">
                            <Path
                                Width="5"
                                Height="8"
                                Margin="1,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Data="{StaticResource ThinArrowRight5x8}"
                                Fill="{telerik1:GreenResource ResourceKey=SelectedBrush}"
                                Stretch="Fill" />
                        </Grid>
                        <Grid
                            x:Name="EditIndicator"
                            Width="16"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Visibility="Collapsed">
                            <Path
                                Height="10"
                                Data="{StaticResource EditIndicatorIcon}"
                                Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                                Stretch="Fill" />
                        </Grid>
                        <Grid
                            x:Name="SelectedEditIndicator"
                            Width="16"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Visibility="Collapsed">
                            <Path
                                Height="10"
                                Data="{StaticResource EditIndicatorIcon}"
                                Fill="{telerik1:GreenResource ResourceKey=SelectedBrush}"
                                Stretch="Fill" />
                        </Grid>
                        <Grid
                            x:Name="InvalidEditableIndicator"
                            Background="Transparent"
                            Visibility="Collapsed">
                            <Grid
                                Width="16"
                                Height="10"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center">
                                <Path
                                    Data="{StaticResource EditIndicatorIcon}"
                                    Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                                    Stretch="Fill" />
                                <Path
                                    Margin="2"
                                    Data="{StaticResource EditableInvertedIcon}"
                                    Fill="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                                    Stretch="Fill" />
                            </Grid>
                            <ToolTipService.ToolTip>
                                <ToolTip
                                    Content="{TemplateBinding Errors}"
                                    Placement="Bottom"
                                    Template="{StaticResource GridViewValidationToolTipTemplate}" />
                            </ToolTipService.ToolTip>
                        </Grid>
                        <Grid
                            x:Name="ErrorIndicator"
                            Width="16"
                            Height="16"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Visibility="Collapsed">
                            <Path
                                Data="{StaticResource ErrorIndicatorIcon}"
                                Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                                Stretch="Fill" />
                            <Path
                                Margin="7,3"
                                Data="{StaticResource ExclamationMarkIcon}"
                                Fill="White"
                                Stretch="Fill" />
                            <ToolTipService.ToolTip>
                                <ToolTip
                                    Content="{TemplateBinding Errors}"
                                    Placement="Bottom"
                                    Template="{StaticResource GridViewValidationToolTipTemplate}" />
                            </ToolTipService.ToolTip>
                        </Grid>
                        <Border
                            x:Name="PART_RowResizer"
                            Height="4"
                            VerticalAlignment="Bottom"
                            Background="Transparent"
                            Cursor="SizeNS" />
                    </Grid>
                </Border>
                <grid:DataCellsPresenter
                    x:Name="PART_DataCellsPresenter"
                    Grid.Column="1"
                    Grid.ColumnSpan="2">
                    <grid:DataCellsPresenter.ItemsPanel>
                        <ItemsPanelTemplate>
                            <treeList:TreeListCellsPanel />
                        </ItemsPanelTemplate>
                    </grid:DataCellsPresenter.ItemsPanel>
                </grid:DataCellsPresenter>
            </grid:SelectiveScrollingGrid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup>
                    <VisualState x:Name="Unselected" />
                    <VisualState x:Name="SelectedUnfocused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Background_Selected_Unfocused"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Background_Selected"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Background_Selected"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Border>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True" />
                    <Condition Property="DisplayVisualCues" Value="True" />
                    <Condition Property="IsValid" Value="True" />
                    <Condition Property="IsSelected" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Over" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsValid" Value="False" />
                    <Condition Property="IsInEditMode" Value="False" />
                    <Condition Property="DisplayVisualCues" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Invalid" Property="Visibility" Value="Visible" />
                <Setter TargetName="ErrorIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="Background_Selected_Unfocused" Property="Background" Value="{x:Null}" />
                <Setter TargetName="Background_Main" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsValid" Value="False" />
                    <Condition Property="IsInEditMode" Value="True" />
                    <Condition Property="DisplayVisualCues" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Invalid" Property="Visibility" Value="Visible" />
                <Setter TargetName="InvalidEditableIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="Background_Selected_Unfocused" Property="Background" Value="{x:Null}" />
                <Setter TargetName="Background_Main" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True" />
                    <Condition Property="IsInEditMode" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="SelectedEditIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="False" />
                    <Condition Property="IsInEditMode" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="EditIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsCurrent" Value="True" />
                    <Condition Property="IsSelected" Value="False" />
                    <Condition Property="IsInEditMode" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="NavigatorIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsCurrent" Value="True" />
                    <Condition Property="IsSelected" Value="True" />
                    <Condition Property="IsInEditMode" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="SelectedNavigatorIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="TreeListViewRowStyle" TargetType="treeList:TreeListViewRow">
        <Setter Property="Template" Value="{StaticResource TreeListViewRowTemplate}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="AllowDrop" Value="True" />
    </Style>
    <Style BasedOn="{StaticResource TreeListViewRowStyle}" TargetType="treeList:TreeListViewRow" />
    <ControlTemplate x:Key="NonEditableComboBox" TargetType="input:RadComboBox">
        <Grid x:Name="VisualRoot">
            <ContentPresenter
                x:Name="Content"
                Grid.Column="0"
                Margin="{TemplateBinding Padding}"
                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                Content="{TemplateBinding SelectionBoxItem}"
                ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                IsHitTestVisible="False" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="LookupElementTemplate" TargetType="grid:LookupElement">
        <input:RadComboBox
            x:Name="PART_RadComboBox"
            Margin="{TemplateBinding Padding}"
            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
            DisplayMemberPath="{TemplateBinding DisplayMemberPath}"
            EmptyText="{TemplateBinding EmptyText}"
            FontFamily="{TemplateBinding FontFamily}"
            FontSize="{TemplateBinding FontSize}"
            FontWeight="{TemplateBinding FontWeight}"
            Foreground="{TemplateBinding Foreground}"
            IsEditable="False"
            IsSynchronizedWithCurrentItem="False"
            IsTabStop="{TemplateBinding IsTabStop}"
            ItemTemplate="{TemplateBinding ItemTemplate}"
