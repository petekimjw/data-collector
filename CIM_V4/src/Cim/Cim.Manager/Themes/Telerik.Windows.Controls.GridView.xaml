            </DataTrigger>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType={x:Type controls:RadGridView}}, Path=ColumnAggregatesAlignment, Mode=OneWay}" Value="BelowGroupKey">
                <Setter TargetName="PART_DataCellsPresenterGrid" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_DataCellsPresenterGrid" Property="Grid.Row" Value="1" />
            </DataTrigger>
            <DataTrigger Binding="{Binding AggregateResults.Count, RelativeSource={RelativeSource Self}}" Value="0">
                <Setter TargetName="PART_DataCellsPresenterGrid" Property="Visibility" Value="Collapsed" />
            </DataTrigger>
            <DataTrigger Binding="{Binding ShowHeaderAggregates, RelativeSource={RelativeSource Self}}" Value="False">
                <Setter TargetName="PART_DataCellsPresenterGrid" Property="Visibility" Value="Collapsed" />
            </DataTrigger>
        </ControlTemplate.Triggers>
