    <ControlTemplate x:Key="GridViewCheckBoxTemplate" TargetType="grid:GridViewCheckBox">
        <Grid
            Width="13"
            Height="13"
            HorizontalAlignment="Left"
            VerticalAlignment="Center">
            <Grid Background="Transparent">
                <Border
                    x:Name="PART_RootElement"
                    Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                    BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                    BorderThickness="1"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}" />
                <Border
                    x:Name="IndeterminatePath"
                    Width="9"
                    Height="9"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Background="{telerik1:GreenResource ResourceKey=MarkerBrush}"
                    CornerRadius="1"
                    Visibility="Collapsed" />
                <Path
                    x:Name="CheckedPath"
                    Width="8"
                    Height="7"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Data="{StaticResource OptionMark}"
                    Stretch="Fill"
                    Stroke="{telerik1:GreenResource ResourceKey=MarkerBrush}"
                    StrokeThickness="1.5"
                    Visibility="Collapsed" />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CheckStates">
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                BeginTime="0"
                                Storyboard.TargetName="CheckedPath"
                                Storyboard.TargetProperty="(UIElement.Visibility)"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Unchecked" />
                    <VisualState x:Name="Indeterminate">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                BeginTime="0"
                                Storyboard.TargetName="IndeterminatePath"
                                Storyboard.TargetProperty="(UIElement.Visibility)"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
