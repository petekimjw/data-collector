                <ToolTip
                    Content="{Binding ButtonTooltip}"
                    Placement="Right"
                    Visibility="{Binding ButtonTooltip, Converter={StaticResource ControlPanelItemButtonTooltipToVisibilityConverter}}" />
            </ToolTipService.ToolTip>
            <Grid Margin="0,3">
                <Border
                    Width="16"
                    Height="2"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
                <Border
                    Width="16"
                    Height="2"
                    Margin="0,4,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
                <Border
                    Width="16"
                    Height="2"
                    Margin="0,8,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
            </Grid>
        </telerik:RadDropDownButton>
    </ControlTemplate>
