            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="DisplayVisualCues" Value="True" />
                    <Condition Property="IsMouseOver" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="SelectionBackground" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True" />
                    <Condition Property="DisplayVisualCues" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="SelectionBackground" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
