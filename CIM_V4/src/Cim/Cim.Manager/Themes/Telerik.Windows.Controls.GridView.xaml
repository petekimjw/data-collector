    <ControlTemplate x:Key="GridViewGroupToggleButtonTemplate" TargetType="ToggleButton">
        <Border
            Background="{TemplateBinding Background}"
            BorderBrush="{TemplateBinding BorderBrush}"
            BorderThickness="{TemplateBinding BorderThickness}">
            <ContentPresenter />
        </Border>
    </ControlTemplate>
    <Style x:Key="GridViewGroupToggleButtonStyle" TargetType="ToggleButton">
        <Setter Property="Template" Value="{StaticResource GridViewGroupToggleButtonTemplate}" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="8,0,0,0" />
    </Style>
    <ControlTemplate x:Key="GridViewGroupRowTemplate" TargetType="grid:GridViewGroupRow">
        <Grid x:Name="PART_GroupExpanderGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition MinHeight="{TemplateBinding MinHeight}" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Border
                x:Name="ToggleButtonBorder"
                Grid.Column="2"
                Grid.ColumnSpan="2"
                MinHeight="{TemplateBinding MinHeight}"
                grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                Background="{TemplateBinding Background}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="26" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Border
                        Grid.Column="2"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                        Visibility="{Binding ShowHeaderAggregates, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <grid:AggregateResultsList
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            ItemsSource="{TemplateBinding AggregateResults}">
                            <grid:AggregateResultsList.ItemTemplate>
                                <DataTemplate>
                                    <grid:GridViewAggregateResultCell AggregateResult="{Binding}" />
                                </DataTemplate>
                            </grid:AggregateResultsList.ItemTemplate>
                            <grid:AggregateResultsList.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal" />
                                </ItemsPanelTemplate>
                            </grid:AggregateResultsList.ItemsPanel>
                        </grid:AggregateResultsList>
                    </Border>
                    <Border
                        x:Name="IconOuterBorder"
                        HorizontalAlignment="Stretch"
                        grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                        Background="Transparent"
                        BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                        BorderThickness="0,0,0,1">
                        <Path
                            x:Name="ExpanderButton"
                            Width="6"
                            Height="4"
                            HorizontalAlignment="Center"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            Data="{StaticResource ArrowUp}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            RenderTransformOrigin="0.5,0.5">
                            <Path.RenderTransform>
                                <RotateTransform Angle="180" />
                            </Path.RenderTransform>
                        </Path>
                    </Border>
                    <ToggleButton
                        x:Name="HeaderButton"
                        Grid.Column="0"
                        Grid.ColumnSpan="3"
                        grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                        Background="{TemplateBinding Background}"
                        IsChecked="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                        IsTabStop="{TemplateBinding IsTabStop}"
                        Opacity="0" />
                    <ToggleButton
                        Grid.Column="1"
                        Padding="0,0,10,0"
                        grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                        Background="Transparent"
                        BorderBrush="{x:Null}"
                        Foreground="{TemplateBinding Foreground}"
                        IsChecked="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                        IsTabStop="{TemplateBinding IsTabStop}"
                        Style="{StaticResource GridViewGroupToggleButtonStyle}">
                        <ContentPresenter
                            Grid.Column="1"
                            Margin="0,0,10,0"
                            VerticalAlignment="Center"
                            Content="{TemplateBinding GroupViewModel}"
                            ContentTemplate="{TemplateBinding GroupHeaderTemplate}"
                            TextBlock.FontFamily="{TemplateBinding FontFamily}"
                            TextBlock.FontSize="{TemplateBinding FontSize}" />
                    </ToggleButton>
                    <Border
                        Grid.Column="1"
                        Grid.ColumnSpan="2"
                        Margin="-1,0,0,0"
                        grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                        BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                        BorderThickness="0,0,0,1" />
                </Grid>
            </Border>
            <Border
                x:Name="PART_IndicatorPresenter"
                Width="26"
                VerticalAlignment="Stretch"
                grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                Background="Transparent"
                BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                BorderThickness="0,0,1,1"
                Visibility="{TemplateBinding RowIndicatorVisibility}" />
            <grid:IndentPresenter
                Grid.Column="1"
                grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                Background="{TemplateBinding Background}"
                BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                BorderThickness="0,0,0,1"
                IndentLevel="{TemplateBinding Level}" />
            <Border
                Grid.Column="2"
                Margin="0,-1,0,0"
                VerticalAlignment="Top"
                BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                BorderThickness="0,1,0,0" />
            <Border
                x:Name="Content"
                Grid.Row="1"
                Grid.ColumnSpan="3"
                Visibility="Collapsed">
                <StackPanel>
                    <grid:GridViewVirtualizingPanel x:Name="PART_GridViewVirtualizingPanel" />
                    <grid:GridViewGroupFooterRow x:Name="Footer" />
                </StackPanel>
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ExpandStateGroup">
                    <VisualState x:Name="Expanded">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Content" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Collapsed" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsExpanded" Value="True">
                <Setter TargetName="IconOuterBorder" Property="BorderThickness" Value="0" />
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames
                                BeginTime="0"
                                Storyboard.TargetName="ExpanderButton"
                                Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames
                                BeginTime="0"
                                Storyboard.TargetName="ExpanderButton"
                                Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)">
                                <EasingDoubleKeyFrame KeyTime="0" Value="180" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
