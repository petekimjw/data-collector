    <Style x:Key="GridViewGroupHeaderCellStyle" TargetType="grid:GridViewGroupHeaderCell">
        <Setter Property="Template" Value="{StaticResource GridViewGroupHeaderCellTemplate}" />
        <Setter Property="Padding" Value="0,0,1,0" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Right" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
