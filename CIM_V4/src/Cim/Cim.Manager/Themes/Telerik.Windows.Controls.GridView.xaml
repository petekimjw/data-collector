    <ControlTemplate x:Key="GridViewRowTemplate" TargetType="grid:GridViewRow">
        <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <grid:SelectiveScrollingGrid x:Name="grid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Border
                    x:Name="Background_Main"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="{TemplateBinding Margin}"
                    Padding="{TemplateBinding Padding}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    Background="{TemplateBinding Background}" />
                <grid:DetailsPresenter
                    x:Name="PART_DetailsPresenter"
                    Grid.Row="1"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="0,-1,0,0"
                    BorderThickness="0"
                    DetailsProvider="{TemplateBinding DetailsProvider}" />
                <ContentPresenter
                    x:Name="PART_HierarchyChildPresenter"
                    Grid.Row="2"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="0,-1,0,0"
                    grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                    Visibility="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Border
                    x:Name="Background_Invalid"
                    Grid.Column="1"
                    Grid.ColumnSpan="4"
                    Background="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                    Opacity="0.3"
                    Visibility="Collapsed" />
                <Border
                    Grid.RowSpan="3"
                    Width="26"
                    grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                    Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                    Visibility="{TemplateBinding RowIndicatorVisibility}" />
                <Border
                    x:Name="PART_RowBorder"
                    Grid.RowSpan="3"
                    Grid.ColumnSpan="4"
                    BorderBrush="{TemplateBinding HorizontalGridLinesBrush}"
                    BorderThickness="{Binding HorizontalGridLinesWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource GridLineWidthToThicknessConverter}, ConverterParameter=Bottom}" />
                <Border
                    x:Name="Background_Over"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,1"
                    Background="{TemplateBinding MouseOverBackground}"
                    Visibility="Collapsed" />
                <Border
                    x:Name="Background_Selected_Unfocused"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,1"
                    Background="{telerik1:GreenResource ResourceKey=BasicBrush}"
                    Opacity="0.6"
                    Visibility="Collapsed" />
                <Border
                    x:Name="Background_Selected"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,1"
                    Background="{TemplateBinding SelectedBackground}"
                    Visibility="Collapsed" />
                <Border
                    x:Name="PART_IndicatorPresenter"
                    Grid.RowSpan="3"
                    Width="26"
                    VerticalAlignment="Stretch"
                    grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                    Background="Transparent"
                    BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                    BorderThickness="0,0,1,0"
                    Visibility="{TemplateBinding RowIndicatorVisibility}">
                    <Grid>
                        <Grid x:Name="GridIndicatorPresenter">
                            <Grid
                                x:Name="NavigatorIndicator"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Visibility="Collapsed">
                                <Path
                                    Width="5"
                                    Height="8"
                                    Margin="1,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Data="{StaticResource ThinArrowRight5x8}"
                                    Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                                    Stretch="Fill" />
                            </Grid>
                            <Grid
                                x:Name="SelectedNavigatorIndicator"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Visibility="Collapsed">
                                <Path
                                    Width="5"
                                    Height="8"
                                    Margin="1,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Data="{StaticResource ThinArrowRight5x8}"
                                    Fill="{telerik1:GreenResource ResourceKey=SelectedBrush}"
                                    Stretch="Fill" />
                            </Grid>
                            <Grid
                                x:Name="EditIndicator"
                                Width="16"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Visibility="Collapsed">
                                <Path
                                    Height="10"
                                    Data="{StaticResource EditIndicatorIcon}"
                                    Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                                    Stretch="Fill" />
                            </Grid>
                            <Grid
                                x:Name="SelectedEditIndicator"
                                Width="16"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Visibility="Collapsed">
                                <Path
                                    Height="10"
                                    Data="{StaticResource EditIndicatorIcon}"
                                    Fill="{telerik1:GreenResource ResourceKey=SelectedBrush}"
                                    Stretch="Fill" />
                            </Grid>
                            <Grid
                                x:Name="InvalidEditableIndicator"
                                Background="Transparent"
                                Visibility="Collapsed">
                                <Grid
                                    Width="16"
                                    Height="10"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <Path
                                        Data="{StaticResource EditIndicatorIcon}"
                                        Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                                        Stretch="Fill" />
                                    <Path
                                        Margin="2"
                                        Data="{StaticResource EditableInvertedIcon}"
                                        Fill="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                                        Stretch="Fill" />
                                </Grid>
                                <ToolTipService.ToolTip>
                                    <ToolTip
                                        Content="{TemplateBinding Errors}"
                                        Placement="Bottom"
                                        Template="{StaticResource GridViewValidationToolTipTemplate}" />
                                </ToolTipService.ToolTip>
                            </Grid>
                            <Grid
                                x:Name="ErrorIndicator"
                                Width="16"
                                Height="16"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Visibility="Collapsed">
                                <Path
                                    Data="{StaticResource ErrorIndicatorIcon}"
                                    Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                                    Stretch="Fill" />
                                <Path
                                    Margin="7,3"
                                    Data="{StaticResource ExclamationMarkIcon}"
                                    Fill="White"
                                    Stretch="Fill" />
                                <ToolTipService.ToolTip>
                                    <ToolTip
                                        Content="{TemplateBinding Errors}"
                                        Placement="Bottom"
                                        Template="{StaticResource GridViewValidationToolTipTemplate}" />
                                </ToolTipService.ToolTip>
                            </Grid>
                            <Thumb x:Name="PART_RowResizer" Style="{StaticResource RowGripperStyle}" />
                        </Grid>
                        <grid:GridViewPinButton
                            x:Name="PinnedRowPresenter"
                            IsPinned="{Binding IsPinned, RelativeSource={RelativeSource TemplatedParent}}"
                            Visibility="Collapsed" />
                    </Grid>
                </Border>
                <grid:DataCellsPresenter x:Name="PART_DataCellsPresenter" Grid.Column="3" />
                <controls:GridViewToggleButton
                    x:Name="PART_HierarchyExpandButton"
                    Grid.Column="2"
                    Width="26"
                    grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                    IsChecked="{Binding IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                    IsHitTestVisible="{TemplateBinding IsExpandable}"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Opacity="{Binding IsExpandable, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToOpacityConverter}}"
                    Visibility="{Binding HasHierarchy, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <grid:IndentPresenter
                    x:Name="PART_IndentPresenter"
                    Grid.RowSpan="4"
                    Grid.Column="1"
                    grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                    IndentLevel="{TemplateBinding IndentLevel}" />
            </grid:SelectiveScrollingGrid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup>
                    <VisualState x:Name="Unselected" />
                    <VisualState x:Name="SelectedUnfocused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Background_Selected_Unfocused"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Background_Selected"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Background_Selected"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Border>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True" />
                    <Condition Property="DisplayVisualCues" Value="True" />
                    <Condition Property="IsValid" Value="True" />
                    <Condition Property="IsSelected" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Over" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsValid" Value="False" />
                    <Condition Property="IsInEditMode" Value="False" />
                    <Condition Property="DisplayVisualCues" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Invalid" Property="Visibility" Value="Visible" />
                <Setter TargetName="ErrorIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="Background_Selected_Unfocused" Property="Background" Value="{x:Null}" />
                <Setter TargetName="Background_Selected" Property="Background" Value="{x:Null}" />
                <Setter TargetName="Background_Main" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsValid" Value="False" />
                    <Condition Property="IsInEditMode" Value="True" />
                    <Condition Property="DisplayVisualCues" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Invalid" Property="Visibility" Value="Visible" />
                <Setter TargetName="InvalidEditableIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="Background_Selected_Unfocused" Property="Background" Value="{x:Null}" />
                <Setter TargetName="Background_Selected" Property="Background" Value="{x:Null}" />
                <Setter TargetName="Background_Main" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True" />
                    <Condition Property="IsInEditMode" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="SelectedEditIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="False" />
                    <Condition Property="IsInEditMode" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="EditIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsCurrent" Value="True" />
                    <Condition Property="IsSelected" Value="False" />
                    <Condition Property="IsInEditMode" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="NavigatorIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsCurrent" Value="True" />
                    <Condition Property="IsSelected" Value="True" />
                    <Condition Property="IsInEditMode" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="SelectedNavigatorIndicator" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding IsEnabled, ElementName=PinnedRowPresenter}" Value="True" />
                    <Condition Binding="{Binding IsMouseOver, ElementName=PART_IndicatorPresenter}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="GridIndicatorPresenter" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PinnedRowPresenter" Property="Visibility" Value="Visible" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding IsPinned, RelativeSource={RelativeSource Self}}" Value="True" />
                    <Condition Binding="{Binding IsEnabled, ElementName=PinnedRowPresenter}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="GridIndicatorPresenter" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PinnedRowPresenter" Property="Visibility" Value="Visible" />
            </MultiDataTrigger>
            <Trigger Property="HorizontalGridLinesWidth" Value="0">
                <Setter TargetName="Background_Over" Property="Margin" Value="0" />
                <Setter TargetName="Background_Selected_Unfocused" Property="Margin" Value="0" />
                <Setter TargetName="Background_Selected" Property="Margin" Value="0" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="GridViewRowStyle" TargetType="grid:GridViewRow">
        <Setter Property="Template" Value="{StaticResource GridViewRowTemplate}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="SelectedBackground" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
        <Setter Property="MouseOverBackground" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="AllowDrop" Value="True" />
    </Style>
    <Style BasedOn="{StaticResource GridViewRowStyle}" TargetType="grid:GridViewRow" />
    <ControlTemplate x:Key="GridViewNewRow_ValidationToolTipTemplate" TargetType="ToolTip">
        <Grid
            x:Name="Root"
            Margin="2,0"
            Background="Transparent"
            Opacity="0"
            RenderTransformOrigin="0 0">
            <Grid.RenderTransform>
                <TranslateTransform x:Name="xform" X="-25" />
            </Grid.RenderTransform>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Border
                Grid.Column="1"
                MinHeight="20"
                Background="{telerik1:GreenResource ResourceKey=ValidationBrush}">
                <ItemsControl ItemsSource="{TemplateBinding Content}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <TextBlock
                                MinHeight="20"
                                MaxWidth="250"
                                Margin="4"
                                VerticalAlignment="Center"
                                FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                                FontSize="{telerik1:GreenResource ResourceKey=FontSize}"
                                Foreground="{telerik1:GreenResource ResourceKey=SelectedBrush}"
                                Text="{Binding}"
                                TextWrapping="Wrap" />
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel />
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Border>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="GridViewNewRowTemplate" TargetType="grid:GridViewNewRow">
        <grid:SelectiveScrollingGrid x:Name="grid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Border
                x:Name="SelectionBackground"
                Grid.Column="2"
                Grid.ColumnSpan="2"
                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                grid:SelectiveScrollingGrid.SelectiveScrollingClip="True" />
            <Border
                Grid.Column="2"
                Width="26"
                grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                BorderBrush="{TemplateBinding VerticalGridLinesBrush}"
                BorderThickness="{Binding VerticalGridLinesWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource GridLineWidthToThicknessConverter}, ConverterParameter=Right}"
                IsHitTestVisible="{Binding IsExpandable, RelativeSource={RelativeSource TemplatedParent}}"
                Opacity="{Binding IsExpandable, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToOpacityConverter}}"
                Visibility="{Binding HasHierarchy, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
            <Border
                x:Name="PART_AlwaysVisibleNewRow"
                Grid.Column="2"
                Grid.ColumnSpan="2"
                Margin="0"
                grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                Background="{TemplateBinding Background}">
                <TextBlock
                    Margin="{TemplateBinding Padding}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    telerik:LocalizationManager.ResourceKey="GridViewAlwaysVisibleNewRow"
                    FontFamily="{TemplateBinding FontFamily}"
                    FontSize="{TemplateBinding FontSize}"
                    Foreground="{TemplateBinding Foreground}" />
            </Border>
            <Border
                Grid.RowSpan="3"
                Width="26"
                grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                Visibility="{TemplateBinding RowIndicatorVisibility}" />
            <Border
                x:Name="PART_RowBorder"
                Grid.RowSpan="4"
                Grid.Column="1"
                Grid.ColumnSpan="4"
                VerticalAlignment="Bottom"
                grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                BorderThickness="0,0,0,1" />
            <grid:DetailsPresenter
                x:Name="PART_DetailsPresenter"
                Grid.Row="1"
                Grid.Column="3"
                DetailsProvider="{TemplateBinding DetailsProvider}" />
            <Border
                x:Name="ValidationLayer"
                Grid.ColumnSpan="4"
                Background="{telerik1:GreenResource ResourceKey=MainBrush}"
                Visibility="Collapsed" />
            <Border
                x:Name="PART_IndicatorPresenter"
                Grid.RowSpan="2"
                Width="26"
                VerticalAlignment="Stretch"
                grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                Background="Transparent"
                BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                BorderThickness="0,0,1,1"
                Visibility="{TemplateBinding RowIndicatorVisibility}">
                <Grid>
                    <Grid
                        x:Name="NewRowIndicator"
                        Width="15"
                        Height="11"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center">
                        <Path
                            Height="4"
                            Margin="0,0,1,0"
                            VerticalAlignment="Top"
                            Data="{StaticResource NewRowIcon}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            Stretch="Fill" />
                        <Path
                            Width="6"
                            Margin="0,5,0,0"
                            HorizontalAlignment="Right"
                            Data="{StaticResource PlusIcon6x6}"
                            Fill="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                            Stretch="Fill" />
                    </Grid>
                    <Grid
                        x:Name="EditIndicator"
                        Width="16"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Visibility="Collapsed">
                        <Path
                            Height="10"
                            Data="{StaticResource EditIndicatorIcon}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            Stretch="Fill" />
                    </Grid>
                    <Grid
                        x:Name="InvalidEditableIndicator"
                        Background="Transparent"
                        Visibility="Collapsed">
                        <Grid
                            Width="16"
                            Height="10"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center">
                            <Path
                                Data="{StaticResource EditIndicatorIcon}"
                                Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                                Stretch="Fill" />
                            <Path
                                Margin="2"
                                Data="{StaticResource EditableInvertedIcon}"
                                Fill="{telerik1:GreenResource ResourceKey=MainBrush}"
                                Stretch="Fill" />
                        </Grid>
                        <ToolTipService.ToolTip>
                            <ToolTip
                                Content="{TemplateBinding Errors}"
                                Placement="Bottom"
                                Template="{StaticResource GridViewNewRow_ValidationToolTipTemplate}" />
                        </ToolTipService.ToolTip>
                    </Grid>
                    <Grid
                        x:Name="ErrorIndicator"
                        Width="16"
                        Height="16"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Visibility="Collapsed">
                        <Path
                            Margin="1"
                            Data="{StaticResource ErrorIndicatorCircleIcon}"
                            Stretch="Fill">
                            <Path.Fill>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Offset="0" Color="#FFFC9999" />
                                    <GradientStop Offset="1" Color="#FFC26666" />
                                </LinearGradientBrush>
                            </Path.Fill>
                        </Path>
                        <Path
                            Margin="7,3"
                            Data="{StaticResource ExclamationMarkIcon}"
                            Fill="White"
                            Stretch="Fill" />
                        <Path Data="{StaticResource ErrorIndicatorIcon}" Stretch="Fill">
                            <Path.Fill>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Offset="0" Color="Red" />
                                    <GradientStop Offset="1" Color="#FF990000" />
                                </LinearGradientBrush>
                            </Path.Fill>
                        </Path>
                        <ToolTipService.ToolTip>
                            <ToolTip
                                x:Name="validationTooltip"
                                Content="{TemplateBinding Errors}"
                                Placement="Bottom"
                                Template="{StaticResource GridViewNewRow_ValidationToolTipTemplate}" />
                        </ToolTipService.ToolTip>
                    </Grid>
                </Grid>
            </Border>
            <Rectangle
                x:Name="Background_Invalid"
                Grid.ColumnSpan="4"
                grid:SelectiveScrollingGrid.SelectiveScrollingClip="True"
                Stroke="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                StrokeThickness="1"
                Visibility="Collapsed" />
            <grid:DataCellsPresenter
                x:Name="PART_DataCellsPresenter"
                Grid.Column="3"
                Visibility="Collapsed" />
            <grid:IndentPresenter
                x:Name="PART_IndentPresenter"
                Grid.RowSpan="2"
                Grid.Column="1"
                grid:SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical"
                IndentLevel="{TemplateBinding IndentLevel}" />
        </grid:SelectiveScrollingGrid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsInEditMode" Value="True">
                <Setter TargetName="NewRowIndicator" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="EditIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_DataCellsPresenter" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_AlwaysVisibleNewRow" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PART_RowBorder" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
            </Trigger>
            <Trigger Property="IsValid" Value="False">
                <Setter TargetName="Background_Invalid" Property="Visibility" Value="Visible" />
                <Setter TargetName="NewRowIndicator" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="EditIndicator" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="ErrorIndicator" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsInEditMode" Value="False">
                <Setter TargetName="PART_AlwaysVisibleNewRow" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_DataCellsPresenter" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="GridViewNewRowStyle" TargetType="grid:GridViewNewRow">
        <Setter Property="MinHeight" Value="26" />
        <Setter Property="Padding" Value="4" />
        <Setter Property="Template" Value="{StaticResource GridViewNewRowTemplate}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
