    <ControlTemplate x:Key="TreeListViewRowCoreTemplate" TargetType="treeList:TreeListViewRow">
        <grid:SelectiveScrollingGrid x:Name="grid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
