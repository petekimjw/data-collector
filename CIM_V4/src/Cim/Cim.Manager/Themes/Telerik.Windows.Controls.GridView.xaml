    <ControlTemplate x:Key="GridViewHeaderCellTemplate" TargetType="grid:GridViewHeaderCell">
        <Grid x:Name="PART_OuterGrid">
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid x:Name="PART_HeaderCellGrid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="4" />
                </Grid.ColumnDefinitions>
                <Border
                    x:Name="GridViewHeaderCell"
                    Grid.ColumnSpan="5"
                    MinHeight="{TemplateBinding MinHeight}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}">
                    <Grid>
                        <Border
                            x:Name="GridViewHeaderCell_Over"
                            Background="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                            Visibility="Collapsed" />
                        <Border
                            x:Name="GridViewHeaderCell_Selected"
                            Background="{telerik1:GreenResource ResourceKey=AccentHighBrush}"
                            Visibility="Collapsed" />
                    </Grid>
                </Border>
                <ContentPresenter
                    Margin="{TemplateBinding Padding}"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Center"
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}"
                    TextBlock.Foreground="{TemplateBinding Foreground}" />
                <ContentControl
                    Grid.Column="1"
                    Margin="0,0,2,4"
                    VerticalAlignment="Center"
                    Content="{Binding SortingIndex, Converter={StaticResource SortingIndexConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                    FontSize="{telerik1:GreenResource ResourceKey=FontSizeXS}"
                    Foreground="{telerik1:GreenResource ResourceKey=SelectedBrush}"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Visibility="{Binding SortingIndex, Converter={StaticResource SortingIndexToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}" />
                <Path
                    x:Name="PART_SortIndicator"
                    Grid.Column="2"
                    Width="6"
                    Height="4"
                    Margin="0,0,0,6"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Data="{StaticResource ArrowUp}"
                    Fill="{telerik1:GreenResource ResourceKey=SelectedBrush}"
                    RenderTransformOrigin="0.5,0.5"
                    SnapsToDevicePixels="True"
                    Stretch="Fill"
                    Visibility="Collapsed" />
                <grid:FilteringDropDown
                    Name="PART_DistinctFilterControl"
                    Grid.Column="3"
                    VerticalAlignment="Center"
                    Visibility="{TemplateBinding FilteringUIVisibility}" />
                <Thumb
                    x:Name="PART_LeftHeaderGripper"
                    Grid.ColumnSpan="5"
                    HorizontalAlignment="Left"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Style="{StaticResource ColumnHeaderGripperStyle}" />
                <Thumb
                    x:Name="PART_RightHeaderGripper"
                    Grid.ColumnSpan="5"
                    HorizontalAlignment="Right"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Style="{StaticResource ColumnHeaderGripperStyle}" />
            </Grid>
            <grid:FieldFilterControl
                x:Name="PART_FieldFilterControl"
                Grid.Row="1"
                Visibility="{TemplateBinding FieldFilterControlVisibility}" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="GridViewHeaderCell_Over" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="SortingState" Value="Ascending">
                <Setter TargetName="GridViewHeaderCell_Selected" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_SortIndicator" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="SortingState" Value="Descending">
                <Setter TargetName="PART_SortIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_SortIndicator" Property="RenderTransform">
                    <Setter.Value>
                        <RotateTransform Angle="180" />
                    </Setter.Value>
                </Setter>
                <Setter TargetName="GridViewHeaderCell_Selected" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="SortingState" Value="None">
                <Setter TargetName="PART_SortIndicator" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
