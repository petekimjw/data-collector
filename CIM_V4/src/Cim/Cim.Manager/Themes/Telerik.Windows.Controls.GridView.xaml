        <Setter Property="Width" Value="7" />
        <Setter Property="Height" Value="9" />
        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Stretch" Value="Fill" />
