            <Border
                BorderBrush="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                BorderThickness="1"
                CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}" />
            <Path
                Width="8"
                Height="8"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Data="{StaticResource ArrowTopRight8x8}"
                Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}" />
            <ToolTipService.ToolTip>
