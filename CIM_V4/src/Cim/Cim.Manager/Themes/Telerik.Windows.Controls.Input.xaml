    </Style>
    <Style x:Key="CalculatorDisplayValue_Style" TargetType="TextBox">
        <Setter Property="Margin" Value="0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="TextAlignment" Value="Right" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
        <Setter Property="HorizontalContentAlignment" Value="Right" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="IsReadOnly" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TextBox">
                    <ContentPresenter
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        Content="{TemplateBinding Text}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="CalculatorButtonStyle"
        BasedOn="{StaticResource RadButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="MinHeight" Value="28" />
        <Setter Property="MinWidth" Value="36" />
        <Setter Property="Margin" Value="2" />
        <Setter Property="Focusable" Value="False" />
    </Style>
    <ControlTemplate x:Key="RadCalculatorTemplate" TargetType="controls:RadCalculator">
        <Grid Background="Transparent" SnapsToDevicePixels="True">
            <Border
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                SnapsToDevicePixels="True">
                <Grid x:Name="Root" Margin="{TemplateBinding Padding}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Border
                        MinHeight="58"
                        Margin="2"
                        Padding="4"
                        BorderBrush="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                        BorderThickness="1"
                        CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                        SnapsToDevicePixels="True">
                        <Grid>
