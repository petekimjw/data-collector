                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="7"
                            Content="7"
                            FontWeight="Bold" />
                        <telerik:RadButton
                            x:Name="input8_Button"
                            Grid.Row="2"
                            Grid.Column="1"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="8"
                            Content="8"
                            FontWeight="Bold" />
                        <telerik:RadButton
                            x:Name="input9_Button"
                            Grid.Row="2"
                            Grid.Column="2"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="9"
                            Content="9"
                            FontWeight="Bold" />
                        <telerik:RadButton
                            x:Name="Divide_Button"
                            Grid.Row="2"
                            Grid.Column="3"
                            Background="Transparent"
                            BorderThickness="0"
                            Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Divide}"
                            Content="/" />
                        <telerik:RadButton
                            x:Name="Percent_Button"
                            Grid.Row="2"
                            Grid.Column="4"
                            Background="Transparent"
