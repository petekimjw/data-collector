                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Border
                                    x:Name="ReadOnlyVisual"
                                    Grid.ColumnSpan="2"
                                    Background="{telerik1:GreenResource ResourceKey=ReadOnlyBackgroundBrush}"
                                    BorderBrush="{x:Null}"
                                    BorderThickness="0"
                                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                                    Opacity="0"
                                    SnapsToDevicePixels="True" />
                                <telerikMaskedInput:PreviewInputTextBox
                                    x:Name="EditorSite"
                                    Height="{TemplateBinding Height}"
                                    Padding="{TemplateBinding Padding}"
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                    AcceptsReturn="{TemplateBinding AcceptsReturn}"
                                    AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                    Background="{TemplateBinding Background}"
                                    CaretBrush="{telerik1:GreenResource ResourceKey=MarkerBrush}"
                                    Cursor="{TemplateBinding Cursor}"
                                    Focusable="{TemplateBinding Focusable}"
                                    FontFamily="{TemplateBinding FontFamily}"
                                    FontSize="{TemplateBinding FontSize}"
                                    FontWeight="{TemplateBinding FontWeight}"
                                    Foreground="{TemplateBinding Foreground}"
                                    IsEnabled="{TemplateBinding IsEnabled}"
                                    IsReadOnly="{TemplateBinding IsReadOnly}"
                                    Style="{TemplateBinding TextBoxStyle}"
                                    TabIndex="{TemplateBinding TabIndex}"
                                    TextAlignment="{Binding HorizontalContentAlignment, Converter={StaticResource conv}, RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                                <Border
                                    x:Name="DisplayTextContent"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="0"
                                    IsHitTestVisible="False"
                                    Visibility="Collapsed">
                                    <telerikMaskedInput:PreviewInputTextBox
                                        x:Name="DisplayText"
                                        Height="{TemplateBinding Height}"
                                        Padding="{TemplateBinding Padding}"
                                        HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                        AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                        Cursor="{TemplateBinding Cursor}"
                                        FontFamily="{TemplateBinding FontFamily}"
                                        FontSize="{TemplateBinding FontSize}"
                                        FontWeight="{TemplateBinding FontWeight}"
                                        Foreground="{TemplateBinding Foreground}"
                                        IsEnabled="{TemplateBinding IsEnabled}"
                                        IsReadOnly="{TemplateBinding IsReadOnly}"
                                        IsTabStop="False"
                                        Style="{TemplateBinding TextBoxStyle}"
                                        Text="{Binding Path=Text, RelativeSource={RelativeSource Mode=TemplatedParent}, Mode=OneWay}"
                                        TextAlignment="{Binding HorizontalContentAlignment, Converter={StaticResource conv}, RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                                </Border>
                                <Border
                                    x:Name="EmptyContent"
                                    Padding="5,0,0,0"
                                    IsHitTestVisible="False"
                                    Visibility="Collapsed">
                                    <ContentControl
                                        x:Name="Content"
                                        Margin="{TemplateBinding Padding}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        Content="{TemplateBinding EmptyContent}"
                                        ContentTemplate="{TemplateBinding EmptyContentTemplate}"
                                        FontStyle="Italic"
                                        Foreground="{TemplateBinding Foreground}"
                                        IsTabStop="False" />
                                </Border>
                                <telerik1:RadButton
                                    x:Name="ClearButton"
                                    Grid.Column="1"
                                    Command="{TemplateBinding ClearCommand}"
                                    IsTabStop="False"
                                    Opacity="0"
                                    Style="{TemplateBinding ClearButtonStyle}"
                                    Visibility="{Binding Path=IsClearButtonVisible, RelativeSource={RelativeSource Mode=TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                            </Grid>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsReadOnly" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=LowBrush}" />
                            <Setter TargetName="DisplayText" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="ReadOnlyVisual" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsKeyboardFocusWithin" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}" />
                            <Setter TargetName="DisplayTextContent" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="EditorSite" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsKeyboardFocusWithin" Value="False">
                            <Setter TargetName="DisplayTextContent" Property="Visibility" Value="Visible" />
                            <Setter TargetName="EditorSite" Property="Opacity" Value="0" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                            <Setter TargetName="ClearButton" Property="Opacity" Value="1" />
                        </Trigger>

                        <!--  Content State - Empty  -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsKeyboardFocusWithin" Value="False" />
                                <Condition Property="IsEmpty" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="EmptyContent" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DisplayTextContent" Property="Opacity" Value="0" />
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="DisplayText" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
