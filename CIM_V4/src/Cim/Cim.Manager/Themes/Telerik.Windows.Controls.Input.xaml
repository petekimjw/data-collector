    <ControlTemplate x:Key="NonEditableComboToggleButtonControlTemplate" TargetType="telerik:RadToggleButton">
        <Grid Background="#01FFFFFF">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Border
                x:Name="ToggleButtonBackground"
                Grid.ColumnSpan="2"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}" />
            <Path
                x:Name="DropDownIcon"
                Grid.Column="1"
                Width="6"
                Height="4"
                Margin="5,0"
                Data="{StaticResource ArrowDown}"
                Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                Stretch="Fill" />
            <Grid Grid.Column="0" Margin="2,0,0,0">
                <ContentPresenter
                    x:Name="Content"
                    Margin="{TemplateBinding Padding}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}">
                    <ContentPresenter.Resources>
                        <Style TargetType="TextBlock">
                            <Setter Property="TextTrimming" Value="CharacterEllipsis" />
                        </Style>
                    </ContentPresenter.Resources>
                </ContentPresenter>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsEnabled" Value="True" />
                    <Condition Property="IsMouseOver" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="ToggleButtonBackground" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                <Setter TargetName="ToggleButtonBackground" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                <Setter TargetName="DropDownIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
            </MultiTrigger>
            <Trigger Property="IsChecked" Value="true">
                <Setter TargetName="ToggleButtonBackground" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="ToggleButtonBackground" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="DropDownIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="DropDownIcon" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="DropDownIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
