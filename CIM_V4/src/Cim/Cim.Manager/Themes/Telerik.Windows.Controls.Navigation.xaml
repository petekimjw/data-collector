                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Border
                    x:Name="HighlightVisual"
                    Grid.ColumnSpan="4"
                    Background="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    BorderBrush="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    BorderThickness="1"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                    Opacity="0"
                    SnapsToDevicePixels="True" />
                <Grid
                    x:Name="IconHost"
                    Width="{TemplateBinding IconColumnWidth}"
                    Margin="-2,0,-1,0">
                    <Path
                        x:Name="Tick"
                        Width="16"
                        Height="16"
                        Margin="3,3,3,3"
                        Data="{StaticResource TickIcon}"
                        Fill="{TemplateBinding Foreground}"
                        FlowDirection="LeftToRight"
                        Visibility="Collapsed" />
                    <ContentPresenter
                        x:Name="Icon"
                        Margin="3,3,3,3"
                        Content="{TemplateBinding Icon}"
                        ContentTemplate="{TemplateBinding IconTemplate}" />
                </Grid>
                <ContentPresenter
                    x:Name="Content"
                    Grid.Column="1"
                    Margin="{TemplateBinding Padding}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    ContentSource="Header"
                    RecognizesAccessKey="True" />
                <telerik:SharedSizeContentPresenter
                    x:Name="PART_InputGestureText"
                    Grid.Column="2"
                    Margin="40,3,3,3"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    Content="{TemplateBinding InputGestureText}"
                    SharedSizeGroupWidth="IGTColumn"
                    Visibility="Collapsed" />
                <telerik:SharedSizeContentPresenter
                    Grid.Column="3"
                    HorizontalAlignment="Right"
                    SharedSizeGroupWidth="ExpandIconColumn">
                    <Grid
                        x:Name="ExpandIcon"
                        Width="16"
                        Height="19"
                        Margin="0,0,2,0"
                        Visibility="Collapsed">
                        <Path
                            x:Name="ExpandIconRight"
                            Width="4"
                            Height="6"
                            Data="{StaticResource ArrowRight}"
                            Fill="{telerik:GreenResource ResourceKey=StrongBrush}"
                            Stretch="Uniform" />
                    </Grid>
                </telerik:SharedSizeContentPresenter>
            </Grid>
            <Popup
                x:Name="PART_Popup"
                AllowsTransparency="True"
                Focusable="False"
                HorizontalOffset="0"
                IsOpen="{TemplateBinding IsSubmenuOpen}"
                VerticalOffset="1">
                <Grid Width="{TemplateBinding DropDownWidth}" Height="{TemplateBinding DropDownHeight}">
                    <Grid x:Name="PopupContentElement">
                        <Border
                            x:Name="PopupBorderVisual"
                            Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                            BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                            BorderThickness="1"
                            CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}">
                            <Grid>
                                <ScrollViewer
                                    x:Name="PART_ScrollViewer"
                                    telerik:SharedSizeScope.IsSharedSizeScope="True"
                                    Style="{StaticResource MenuScrollViewer}">
                                    <Grid>
                                        <ItemsPresenter />
                                    </Grid>
                                </ScrollViewer>
                            </Grid>
                        </Border>
                    </Grid>
                </Grid>
            </Popup>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="RoleStates">
                    <VisualState x:Name="TopLevelItem">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="ContentGrid"
                                Storyboard.TargetProperty="Margin"
                                Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="IconHost"
                                Storyboard.TargetProperty="Margin"
                                Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>-2 0 -7 0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="TopLevelHeader">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="ContentGrid"
                                Storyboard.TargetProperty="Margin"
                                Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="IconHost"
                                Storyboard.TargetProperty="Margin"
                                Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>-2 0 -7 0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="SubmenuItem" />
                    <VisualState x:Name="SubmenuHeader">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="ExpandIcon"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Separator" />
                </VisualStateGroup>
                <VisualStateGroup x:Name="OrientationStates">
                    <VisualState x:Name="Horizontal" />
                    <VisualState x:Name="Vertical">
                        <Storyboard>
                            <DoubleAnimation
                                Storyboard.TargetName="PART_Popup"
                                Storyboard.TargetProperty="HorizontalOffset"
                                To="1"
                                Duration="0:0:0" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="ChildrenStates">
                    <VisualState x:Name="HasChildren" />
                    <VisualState x:Name="NoChildren" />
                </VisualStateGroup>
                <VisualStateGroup x:Name="HeaderStates">
                    <VisualState x:Name="VisibleHeader" />
                    <VisualState x:Name="EmptyHeader">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Icon"
                                Storyboard.TargetProperty="Margin"
                                Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>5 3 4 3</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="Content"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="InputGestureTextStates">
                    <VisualState x:Name="EmptyInputGestureText" />
                    <VisualState x:Name="VisibleInputGestureText">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="PART_InputGestureText"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsHighlighted" Value="True">
                <Setter TargetName="HighlightVisual" Property="Visibility" Value="Visible" />
                <Setter TargetName="HighlightVisual" Property="Opacity" Value="1" />
                <Setter TargetName="ExpandIconRight" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="ExpandIconRight" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="ExpandIconRight" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="Icon" Value="{x:Null}" />
                    <Condition Property="IconTemplate" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter TargetName="Icon" Property="Visibility" Value="Collapsed" />
                </MultiTrigger.Setters>
            </MultiTrigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="Tick" Property="Visibility" Value="Visible" />
                <Setter TargetName="Icon" Property="Visibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="IsSubmenuOpen" Value="True">
                <Setter TargetName="HighlightVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="HighlightVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="HighlightVisual" Property="Opacity" Value="1" />
                <Setter TargetName="ExpandIconRight" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="Content" Property="TextBlock.Foreground" Value="{telerik:GreenResource ResourceKey=SelectedBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="GroupTemplate" TargetType="telerikNavigation:RadMenuGroupItem">
        <Border
            Background="{TemplateBinding Background}"
            BorderBrush="{TemplateBinding BorderBrush}"
            BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Border
                    x:Name="Header"
                    Margin="1,0"
                    Background="{telerik1:GreenResource ResourceKey=StrongBrush}"
                    Opacity="0.2"
                    SnapsToDevicePixels="True" />
                <ContentPresenter
                    x:Name="Content"
                    Margin="4,3,4,4"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    ContentSource="Header"
                    RecognizesAccessKey="True"
                    TextBlock.FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                    TextBlock.Foreground="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <ScrollViewer
                    x:Name="PART_ScrollViewer"
                    Grid.Row="1"
                    Margin="0"
                    Padding="0"
                    Background="Transparent"
                    BorderThickness="0"
                    Focusable="False"
                    HorizontalScrollBarVisibility="{Binding (ScrollViewer.HorizontalScrollBarVisibility), RelativeSource={RelativeSource TemplatedParent}}"
                    VerticalScrollBarVisibility="{Binding (ScrollViewer.VerticalScrollBarVisibility), RelativeSource={RelativeSource TemplatedParent}}">
                    <ItemsPresenter />
                </ScrollViewer>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="HeaderStates">
                    <VisualState x:Name="EmptyHeader">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Header" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Content" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="VisibleHeader" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="SeparatorTemplate" TargetType="telerikNavigation:RadMenuItem">
        <Grid>
            <Grid x:Name="HorizontalSeparator">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid
                    Grid.Column="0"
                    Width="{TemplateBinding IconColumnWidth}"
                    HorizontalAlignment="Left" />
                <Grid
                    Grid.ColumnSpan="2"
                    Height="1"
                    Margin="1">
                    <Rectangle
                        Height="1"
                        VerticalAlignment="Bottom"
                        Fill="{telerik:GreenResource ResourceKey=BasicBrush}" />
                </Grid>
            </Grid>
            <Grid
                x:Name="VerticalSeparator"
                Width="1"
                Margin="0,1"
                Visibility="Collapsed">
                <Rectangle
                    Width="1"
                    HorizontalAlignment="Right"
                    Fill="{telerik:GreenResource ResourceKey=BasicBrush}" />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="OrientationStates">
                    <VisualState x:Name="VerticalState" />
                    <VisualState x:Name="HorizontalState">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="HorizontalSeparator"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="VerticalSeparator"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="MenuTemplate" TargetType="telerikNavigation:RadMenu">
        <Border
            Background="{TemplateBinding Background}"
            BorderBrush="{TemplateBinding BorderBrush}"
            BorderThickness="{TemplateBinding BorderThickness}"
            SnapsToDevicePixels="True">
            <ItemsPresenter Margin="{TemplateBinding Padding}" />
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="ContextMenuTemplate" TargetType="telerikNavigation:RadContextMenu">
        <Grid>
            <Grid x:Name="PopupContentElement">
                <Border
                    x:Name="TemplateBorder"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=BorderThickness}"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}">
                    <Grid>
                        <ScrollViewer
                            x:Name="PART_ScrollViewer"
                            telerik:SharedSizeScope.IsSharedSizeScope="True"
                            Style="{StaticResource MenuScrollViewer}">
                            <Grid>
                                <ItemsPresenter />
                            </Grid>
                        </ScrollViewer>
                    </Grid>
                </Border>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="PopupHostStates">
                    <VisualState x:Name="Standalone" />
                    <VisualState x:Name="Hosted">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PopupContentElement" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="TemplateBorder" Storyboard.TargetProperty="BorderThickness">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadMenuItemBaseStyle" TargetType="telerikNavigation:RadMenuItem">
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=AlternativeBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
    </Style>
    <Style
        x:Key="RadMenuItemStyle"
        BasedOn="{StaticResource RadMenuItemBaseStyle}"
        TargetType="telerikNavigation:RadMenuItem">
        <Setter Property="Padding" Value="9,3" />
        <Setter Property="Template" Value="{StaticResource MenuItemTemplate}" />
        <Setter Property="TopLevelHeaderTemplateKey" Value="{StaticResource MenuItemTemplate}" />
        <Setter Property="TopLevelItemTemplateKey" Value="{StaticResource MenuItemTemplate}" />
        <Setter Property="SubmenuHeaderTemplateKey" Value="{StaticResource MenuItemTemplate}" />
        <Setter Property="SubmenuItemTemplateKey" Value="{StaticResource MenuItemTemplate}" />
        <Setter Property="SeparatorTemplateKey" Value="{StaticResource SeparatorTemplate}" />
        <Setter Property="ScrollViewer.PanningMode" Value="VerticalFirst" />
        <Style.Triggers>
            <Trigger Property="Role" Value="TopLevelHeader">
                <Setter Property="Template" Value="{Binding Path=TopLevelHeaderTemplateKey, RelativeSource={RelativeSource Self}}" />
            </Trigger>
            <Trigger Property="Role" Value="TopLevelItem">
                <Setter Property="Template" Value="{Binding Path=TopLevelItemTemplateKey, RelativeSource={RelativeSource Self}}" />
            </Trigger>
            <Trigger Property="Role" Value="SubmenuHeader">
                <Setter Property="Template" Value="{Binding Path=SubmenuHeaderTemplateKey, RelativeSource={RelativeSource Self}}" />
            </Trigger>
            <Trigger Property="Role" Value="SubmenuItem">
                <Setter Property="Template" Value="{Binding Path=SubmenuItemTemplateKey, RelativeSource={RelativeSource Self}}" />
            </Trigger>
            <Trigger Property="Role" Value="Separator">
                <Setter Property="Template" Value="{Binding Path=SeparatorTemplateKey, RelativeSource={RelativeSource Self}}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
        x:Key="RadMenuGroupItemStyle"
        BasedOn="{StaticResource RadMenuItemBaseStyle}"
        TargetType="telerikNavigation:RadMenuGroupItem">
        <Setter Property="telerik:SharedSizeScope.IsSharedSizeScope" Value="True" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Template" Value="{StaticResource GroupTemplate}" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Disabled" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
    </Style>
    <Style
        x:Key="RadMenuSeparatorItemStyle"
        BasedOn="{StaticResource RadMenuItemBaseStyle}"
        TargetType="telerikNavigation:RadMenuSeparatorItem">
        <Setter Property="Template" Value="{StaticResource SeparatorTemplate}" />
    </Style>
    <Style x:Key="RadMenuStyle" TargetType="telerikNavigation:RadMenu">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=HighBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=HighBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="IconColumnWidth" Value="28" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerik:RadWrapPanel IsAnimated="False" Orientation="{Binding Orientation, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikNavigation:RadMenu}}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource MenuTemplate}" />
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:SlideAnimation
                        AnimationName="Expand"
                        Direction="In"
                        SlideMode="Top"
                        TargetElementName="PopupContentElement" />
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadContextMenuStyle" TargetType="telerikNavigation:RadContextMenu">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="1" />
        <Setter Property="IconColumnWidth" Value="28" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ScrollViewer.PanningMode" Value="VerticalFirst" />
        <Setter Property="Template" Value="{StaticResource ContextMenuTemplate}" />
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:SlideAnimation
                        AnimationName="Expand"
                        Direction="In"
                        SlideMode="Top"
                        TargetElementName="PopupContentElement" />
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource RadMenuItemStyle}" TargetType="telerikNavigation:RadMenuItem" />
    <Style BasedOn="{StaticResource RadMenuGroupItemStyle}" TargetType="telerikNavigation:RadMenuGroupItem" />
    <Style BasedOn="{StaticResource RadMenuSeparatorItemStyle}" TargetType="telerikNavigation:RadMenuSeparatorItem" />
    <Style BasedOn="{StaticResource RadMenuStyle}" TargetType="telerikNavigation:RadMenu" />
    <Style BasedOn="{StaticResource RadContextMenuStyle}" TargetType="telerikNavigation:RadContextMenu" />
    <Style x:Key="RadialMenuButtonStyle" TargetType="telerikRadialMenu:RadialMenuButton">
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="MinWidth" Value="0" />
        <Setter Property="Width" Value="48" />
        <Setter Property="Height" Value="48" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="FontWeight" Value="Normal" />
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Path
                        Width="24"
                        Height="24"
                        Data="{StaticResource RadialMenuButton}"
                        Fill="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                        Stretch="Fill" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="BackContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Path
                        Width="18"
                        Height="14"
                        Margin="0,4,0,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Data="{StaticResource RadialMenuBackButton}"
                        Fill="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                        Stretch="Fill" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:RadialMenuButton">
                    <Grid x:Name="RootGrid">
                        <Grid>
                            <Ellipse
                                x:Name="BackgroundGlyph"
                                Width="{TemplateBinding Width}"
                                Height="{TemplateBinding Height}"
                                Fill="{TemplateBinding Background}"
                                RenderTransformOrigin=".5,.5"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Left}" />
                            <ContentPresenter
                                x:Name="NormalGlyph"
                                Margin="0,-1,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding Content}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                RenderTransformOrigin="0.5,0.5" />
                            <ContentPresenter
                                x:Name="BackGlyph"
                                Margin="0,-3,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                ContentTemplate="{Binding Path=BackContentTemplate, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                                RenderTransformOrigin="0.5,0.5"
                                Visibility="Collapsed" />
                        </Grid>
                        <VisualStateManager.VisualStateGroups>
