                <ControlTemplate TargetType="telerikNavigation:RadNavigationViewItem">
                    <Grid x:Name="ContainerGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="{Binding RelativeSource={RelativeSource AncestorType=telerikNavigation:RadNavigationView}, Path=CompactPaneWidth}" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Border
                            x:Name="HighlightedVisual"
                            Grid.ColumnSpan="2"
                            Background="{TemplateBinding mat:MaterialAssist.MouseOverBrush}"
                            BorderBrush="{StaticResource NavigationView_NavItemBorderBrush_MouseOver}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Opacity="0" />
                        <Border
                            x:Name="SelectedVisual"
                            Grid.ColumnSpan="2"
                            Background="{TemplateBinding mat:MaterialAssist.PressedBrush}"
                            BorderBrush="{StaticResource NavigationView_NavItemBorderBrush_Selected}"
                            BorderThickness="0"
                            Opacity="0" />
                        <Border
                            x:Name="BackgroundVisual"
                            Grid.ColumnSpan="2"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                            SnapsToDevicePixels="True" />
                        <ContentPresenter
                            x:Name="IconContent"
                            Grid.Column="0"
                            ContentSource="Icon"
                            TextElement.Foreground="{telerik:GreenResource ResourceKey=StrongBrush}" />
                        <ContentPresenter
                            x:Name="Content"
                            Grid.Column="1"
                            Margin="{TemplateBinding Padding}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            RecognizesAccessKey="True"
                            TextElement.Foreground="{TemplateBinding Foreground}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsHighlighted" Value="True">
                            <Setter TargetName="IconContent" Property="TextBlock.Foreground" Value="{StaticResource NavigationView_NavItemForeground_Highlighted}" />
                            <Setter TargetName="HighlightedVisual" Property="Opacity" Value="1" />
                            <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_MouseOver}" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="IconContent" Property="TextBlock.Foreground" Value="{StaticResource NavigationView_NavItemForeground_Highlighted}" />
                            <Setter TargetName="Content" Property="TextElement.Foreground" Value="{StaticResource NavigationView_NavItemForeground_Highlighted}" />
                            <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                            <Setter TargetName="SelectedVisual" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="IconContent" Property="TextBlock.Foreground" Value="{StaticResource NavigationView_NavItemForeground_Highlighted}" />
                            <Setter TargetName="Content" Property="TextElement.Foreground" Value="{StaticResource NavigationView_NavItemForeground_Highlighted}" />
                            <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                            <Setter TargetName="SelectedVisual" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContainerGrid" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
