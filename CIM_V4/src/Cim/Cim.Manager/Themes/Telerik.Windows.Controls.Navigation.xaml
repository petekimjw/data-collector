        <Setter Property="Height" Value="{Binding ContextItem.ActualHeight, RelativeSource={RelativeSource Self}}" />
        <Setter Property="Background" Value="{Binding ContextItem, RelativeSource={RelativeSource Self}, Converter={StaticResource VisualToVisualBrushConverter}}" />
