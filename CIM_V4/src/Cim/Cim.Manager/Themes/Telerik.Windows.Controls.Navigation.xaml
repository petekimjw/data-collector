                        x:Name="arrowPageLeft"
                        Width="9"
                        Height="9"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Data="{StaticResource ArrowPageLeft}"
                        Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
