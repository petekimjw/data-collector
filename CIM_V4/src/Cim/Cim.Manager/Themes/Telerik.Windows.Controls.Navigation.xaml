                                    Orientation="Horizontal">
                                    <telerik:RadRadioButton
                                        Content="{StaticResource VerticalAlignmentTopIcon}"
                                        IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Top, Mode=TwoWay}"
                                        Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Top}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                        Content="{StaticResource VerticalAlignmentCenterIcon}"
                                        IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Center, Mode=TwoWay}"
                                        Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Center}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                        Content="{StaticResource VerticalAlignmentBottomIcon}"
                                        IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Bottom, Mode=TwoWay}"
                                        Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Bottom}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                        Margin="0"
                                        Content="{StaticResource VerticalAlignmentStretchIcon}"
                                        IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Stretch, Mode=TwoWay}"
                                        Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Stretch}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                </StackPanel>
                                <Border
                                    x:Name="SeparatorBorder"
