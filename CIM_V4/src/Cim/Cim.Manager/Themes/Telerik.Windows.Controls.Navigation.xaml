                        SnapsToDevicePixels="True"
                        Stretch="Fill" />
                </Grid>
                <Grid
                    x:Name="PageRight"
                    Width="9"
                    Height="9"
                    Background="Transparent"
                    Opacity="0">
                    <Path
                        x:Name="arrowPageRight"
                        Width="9"
                        Height="9"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Data="{StaticResource ArrowPageRight}"
                        Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                        SnapsToDevicePixels="True"
                        Stretch="Fill" />
                </Grid>
                <Grid
                    x:Name="PageUp"
                    Width="9"
                    Height="9"
                    Background="Transparent"
                    Opacity="0">
                    <Path
                        x:Name="arrowPageUp"
                        Width="9"
                        Height="9"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Data="{StaticResource ArrowPageUp}"
                        Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                        SnapsToDevicePixels="True"
                        Stretch="Fill" />
                </Grid>
                <Grid
                    x:Name="LineUp"
                    Width="9"
                    Height="9"
                    Background="Transparent"
                    Opacity="0">
                    <Path
                        x:Name="arrowLineUp"
                        Width="9"
                        Height="7"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Data="{StaticResource ArrowUp9x7}"
                        Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                        SnapsToDevicePixels="True"
                        Stretch="Fill" />
                </Grid>
                <Grid
                    x:Name="LineDown"
                    Width="9"
                    Height="9"
                    Background="Transparent"
                    Opacity="0">
                    <Path
                        x:Name="arrowLineDown"
                        Width="9"
                        Height="7"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Data="{StaticResource ArrowDown9x7}"
                        Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                        SnapsToDevicePixels="True"
                        Stretch="Fill" />
                </Grid>
                <Grid
                    x:Name="PageDown"
                    Width="9"
                    Height="9"
                    Background="Transparent"
                    Opacity="0">
                    <Path
                        x:Name="arrowPageDown"
                        Width="9"
                        Height="9"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Data="{StaticResource ArrowPageDown}"
                        Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                        SnapsToDevicePixels="True"
                        Stretch="Fill" />
                </Grid>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="Arrow" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                <Setter TargetName="Arrow" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                <Setter TargetName="Arrow" Property="BorderThickness" Value="1" />
                <Setter TargetName="arrowPageLeft" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineLeft" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineRight" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowPageRight" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowPageUp" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineUp" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineDown" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowPageDown" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="Arrow" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="Arrow" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="Arrow" Property="BorderThickness" Value="1" />
                <Setter TargetName="arrowPageLeft" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineLeft" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineRight" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowPageRight" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowPageUp" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineUp" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowLineDown" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                <Setter TargetName="arrowPageDown" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter TargetName="Arrow" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}" />
                <Setter TargetName="Arrow" Property="BorderThickness" Value="1" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="LineUp">
                <Setter TargetName="LineUp" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="LineDown">
                <Setter TargetName="LineDown" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="LineLeft">
                <Setter TargetName="LineLeft" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="LineRight">
                <Setter TargetName="LineRight" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="PageUp">
                <Setter TargetName="PageUp" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="PageDown">
                <Setter TargetName="PageDown" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="PageLeft">
                <Setter TargetName="PageLeft" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
            <Trigger Property="CarouselScrollButtonType" Value="PageRight">
                <Setter TargetName="PageRight" Property="Opacity" Value="1" />
                <Setter Property="Width" Value="22" />
                <Setter Property="Height" Value="22" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="CarouselScrollButtonStyle" TargetType="{x:Type carousel:CarouselScrollButton}">
        <Setter Property="Template" Value="{StaticResource CarouselScrollButtonTemplate}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Height" Value="22" />
        <Setter Property="Width" Value="22" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
