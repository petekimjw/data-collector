    <Style x:Key="TileViewMaximizeToggleStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=StrongBrush}" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="MinWidth" Value="18" />
        <Setter Property="MinHeight" Value="18" />
        <Setter Property="CornerRadius" Value="{telerik1:GreenResource ResourceKey=CornerRadius}" />
        <Setter Property="Margin" Value="4,0,0,0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Grid Background="Transparent" SnapsToDevicePixels="True">
                        <Border x:Name="RestoreIconWrapper" CornerRadius="{TemplateBinding CornerRadius}">
                            <ToolTipService.ToolTip>
                                <ToolTip Content="{telerik:LocalizableResource Key=TileViewItemMaximizeText}" />
                            </ToolTipService.ToolTip>
                            <Path
                                x:Name="RestoreIcon"
                                Width="10"
                                Height="10"
                                Data="{StaticResource RestoreFillIcon}"
                                Fill="{TemplateBinding Foreground}"
                                UseLayoutRounding="True" />
                        </Border>
                        <Border
                            x:Name="CollapseIconWrapper"
                            CornerRadius="{TemplateBinding CornerRadius}"
                            Visibility="Collapsed">
                            <ToolTipService.ToolTip>
                                <ToolTip Content="{telerik:LocalizableResource Key=TileViewItemMinimizeText}" />
                            </ToolTipService.ToolTip>
                            <Rectangle
                                x:Name="CollapseIcon"
                                Width="10"
                                Height="3"
                                Margin="0,0,0,3"
                                VerticalAlignment="Bottom"
                                Fill="{TemplateBinding Foreground}" />
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="RestoreIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                            <Setter TargetName="CollapseIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                            <Setter TargetName="RestoreIconWrapper" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                            <Setter TargetName="CollapseIconWrapper" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="RestoreIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                            <Setter TargetName="CollapseIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                            <Setter TargetName="RestoreIconWrapper" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                            <Setter TargetName="CollapseIconWrapper" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="RestoreIconWrapper" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="CollapseIconWrapper" Property="Visibility" Value="Visible" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TileViewItemHeaderStyle" TargetType="Telerik_Windows_Controls_TileView:TileViewItemHeader">
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Margin" Value="-1,-1,-1,0" />
        <Setter Property="Padding" Value="4,0" />
        <Setter Property="MinHeight" Value="28" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Telerik_Windows_Controls_TileView:TileViewItemHeader">
                    <Grid HorizontalAlignment="{TemplateBinding HorizontalAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}">
                        <Border
                            x:Name="BorderVisual"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid x:Name="GripBarElement" Background="Transparent">
                                <Grid Margin="{TemplateBinding Padding}">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <ContentPresenter
                                        x:Name="HeaderElement"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        ContentTemplate="{TemplateBinding ContentTemplate}"
                                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" />
                                    <telerik:RadToggleButton
                                        x:Name="MaximizeToggleButton"
                                        Grid.Column="1"
                                        Command="{x:Static Telerik_Windows_Controls_TileView:TileViewCommands.ToggleTileState}"
                                        Style="{StaticResource TileViewMaximizeToggleStyle}"
                                        Visibility="{Binding RelativeSource={RelativeSource Self}, Path=IsEnabled, Converter={StaticResource BooleanToVisibilityConverter}}" />
                                </Grid>
                            </Grid>
                        </Border>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="SelectionStates">
                                <VisualState x:Name="Selected">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderVisual" Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderVisual" Storyboard.TargetProperty="BorderBrush">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MaximizeToggleButton" Storyboard.TargetProperty="Foreground">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="UnSelected" />
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="TileState" Value="Maximized">
                            <Setter TargetName="MaximizeToggleButton" Property="IsChecked" Value="True" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadTileViewItemStyle" TargetType="telerikNavigation:RadTileViewItem">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="5" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadTileViewItem">
                    <Grid x:Name="PART_MainGrid" SnapsToDevicePixels="True">
                        <Border
                            x:Name="outerContainer"
                            Margin="{TemplateBinding Padding}"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadiusBottom}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Telerik_Windows_Controls_TileView:TileViewItemHeader
                                    x:Name="HeaderPart"
                                    Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}" />
                                <ContentPresenter
                                    x:Name="ContentElement"
                                    Grid.Row="1"
                                    Margin="4"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    Content="{TemplateBinding Content}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"
                                    ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" />
                            </Grid>
                        </Border>
                        <Border
                            x:Name="MouseOverDraggingVisual"
