                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <StackPanel
                                    Margin="0,10,0,0"
                                    HorizontalAlignment="Center"
                                    Orientation="Horizontal">
                                    <telerik:RadRadioButton
                                        Content="{StaticResource HorizontalAlignmentLeftIcon}"
                                        IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Left, Mode=TwoWay}"
