                                    <StackPanel x:Name="HeaderButtons" Orientation="Horizontal">
                                        <telerik:RadButton
                                            x:Name="PART_MinimizeButton"
                                            Command="{x:Static telerik:WindowCommands.Minimize}"
                                            Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Minimize" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Glyph="{StaticResource GlyphMinimize}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton
                                            x:Name="PART_RestoreButton"
                                            Command="{x:Static telerik:WindowCommands.Restore}"
                                            Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Restore" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Glyph="{StaticResource GlyphWindowCollapse}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton
                                            x:Name="PART_MaximizeButton"
                                            Command="{x:Static telerik:WindowCommands.Maximize}"
                                            Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Maximize" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Glyph="{StaticResource GlyphWindow}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton
                                            x:Name="PART_CloseButton"
                                            Margin="0,0,1,0"
                                            Command="{x:Static telerik:WindowCommands.Close}"
                                            Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Close" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Glyph="{StaticResource GlyphClose}" />
                                        </telerik:RadButton>
                                    </StackPanel>
                                </Border>
                            </tabbedWindow:TabbedWindowTabControl.AdditionalContent>
                        </tabbedWindow:TabbedWindowTabControl>
                    </Grid>
                </AdornerDecorator>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter TargetName="WindowOuterBorder" Property="BorderThickness" Value="0" />
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
                <Setter TargetName="Header" Property="Margin" Value="0" />
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left" />
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top" />
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="HeaderContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="HeaderContent" Property="ContentTemplate" Value="{x:Null}" />
                    <Condition SourceName="HeaderContent" Property="ContentTemplateSelector" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="IconContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="IconContent" Property="ContentTemplate" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="IconContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style
        x:Key="RadTabbedWindowStyle"
        BasedOn="{StaticResource RadWindowStyle}"
        TargetType="telerikNavigation:RadTabbedWindow">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2" />
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="0" />
        <Setter Property="Template" Value="{StaticResource RadTabbedWindowTemplate}" />
        <Setter Property="AddButtonStyle" Value="{StaticResource AddItemsButtonStyle}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Header" Value="{x:Null}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="1,0,1,1" />
        <Setter Property="MinHeight" Value="32" />
        <Setter Property="MinWidth" Value="150" />
        <Setter Property="ItemContainerStyle" Value="{StaticResource RadTabbedWindowTabItemStyle}" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigationPrimitives:RadBrowserPanel ItemMinWidth="{Binding ItemMinWidth, FallbackValue=80, RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadTabbedWindow}}}" ItemWidth="{Binding ItemWidth, FallbackValue=120, RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadTabbedWindow}}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="navigation:RadWindowInteropHelper.ShowInTaskbar" Value="True" />
    </Style>
    <Style BasedOn="{StaticResource RadTabbedWindowStyle}" TargetType="telerikNavigation:RadTabbedWindow" />
</ResourceDictionary>
