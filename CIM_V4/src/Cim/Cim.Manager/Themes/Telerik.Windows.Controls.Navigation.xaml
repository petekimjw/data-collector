                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Left}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                        Content="{StaticResource HorizontalAlignmentCenterIcon}"
                                        IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Center, Mode=TwoWay}"
                                        Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Center}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                        Content="{StaticResource HorizontalAlignmentRightIcon}"
                                        IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Right, Mode=TwoWay}"
                                        Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Right}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                        Margin="0"
                                        Content="{StaticResource HorizontalAlignmentStretchIcon}"
                                        IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Stretch, Mode=TwoWay}"
                                        Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Stretch}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="1"
                                    Margin="0,8,0,10"
