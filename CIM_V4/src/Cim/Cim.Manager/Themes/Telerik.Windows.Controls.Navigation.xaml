        <Setter Property="MinHeight" Value="8" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlDropIndicator">
                    <Border
                        x:Name="BorderVisual"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
