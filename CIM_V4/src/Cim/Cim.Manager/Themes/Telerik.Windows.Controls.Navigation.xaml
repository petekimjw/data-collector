    <Style x:Key="TreeViewDragVisualStyle" TargetType="treeView:TreeViewDragVisual">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="4,0" />
        <Setter Property="IsHitTestVisible" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="treeView:TreeViewDragVisual">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                            Visibility="{TemplateBinding DragPreviewVisibility}" />
                        <ContentControl
                            Margin="{TemplateBinding Padding}"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}"
                            Visibility="{TemplateBinding DragPreviewVisibility}" />
                        <Grid
                            Grid.Row="1"
                            MinHeight="22"
                            Visibility="{TemplateBinding DragTooltipVisibility}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Border
                                Grid.ColumnSpan="3"
                                Background="{telerik1:GreenResource ResourceKey=AlternativeBrush}"
                                BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                                BorderThickness="1"
                                CornerRadius="0 1 1 1" />
                            <Grid Margin="5,0,0,0" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsDropPossible, Converter={StaticResource InvertedBooleanToVisibilityConverter}}">
                                <Path
                                    Width="10"
                                    Height="10"
                                    Data="{StaticResource NoneEffect}"
                                    Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                                    Stretch="Fill" />
                            </Grid>
                            <Grid Margin="5,0" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsDropPossible, Converter={StaticResource BooleanToVisibilityConverter}}">
                                <Path
                                    Width="10"
                                    Height="8"
                                    Margin="4,7"
                                    Data="{StaticResource DropPossibleIcon}"
                                    Fill="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                                    Stretch="Fill" />
                            </Grid>
                            <TextBlock
                                Grid.Column="1"
                                VerticalAlignment="Center"
                                Foreground="{TemplateBinding Foreground}"
                                Text="{Binding DropActionText, RelativeSource={RelativeSource TemplatedParent}, StringFormat='{}{0} '}" />
                            <ContentControl
                                Grid.Column="2"
                                Margin="0,0,5,0"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding DropTarget}"
                                ContentTemplate="{TemplateBinding DropTargetTemplate}"
                                Foreground="{TemplateBinding Foreground}" />
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TreeViewDragCueStyle" TargetType="treeView:TreeViewDragCue">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
