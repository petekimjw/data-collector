    <Style x:Key="RadNavigationViewStyle" TargetType="telerikNavigation:RadNavigationView">
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="PaneBackground" Value="{telerik:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="PaneHeaderBackground" Value="{telerik:GreenResource ResourceKey=PrimaryBrush}" />
        <Setter Property="PaneHeaderForeground" Value="{telerik:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=MainBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="PaneHeaderHeight" Value="36" />
        <Setter Property="CompactPaneWidth" Value="40" />
        <Setter Property="ExpandedPaneWidth" Value="250" />
