    <Style BasedOn="{StaticResource RadCarouselStyle}" TargetType="{x:Type telerikNavigation:RadCarousel}" />
    <sys:Double x:Key="WizardButtonMinWidth">90</sys:Double>
    <sys:Double x:Key="WizardButtonMinHeight">22</sys:Double>
    <sys:Double x:Key="WizardFooterMinHeight">40</sys:Double>
    <DataTemplate x:Key="FooterTemplate">
        <Grid Margin="8">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <telerik:RadButton
                x:Name="PART_Help"
                MinWidth="{StaticResource WizardButtonMinWidth}"
                MinHeight="{StaticResource WizardButtonMinHeight}"
                Margin="2,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Command="{x:Static wizard:RadWizardCommands.Help}"
                Content="{Binding HelpButtonContent, Mode=TwoWay}"
                Visibility="{Binding NavigationButtonsVisibility.HelpButtonVisibility}" />
            <StackPanel
                Grid.Column="1"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <telerik:RadButton
                    x:Name="PART_Previous"
                    MinWidth="{StaticResource WizardButtonMinWidth}"
                    MinHeight="{StaticResource WizardButtonMinHeight}"
                    Margin="2,0"
                    VerticalAlignment="Center"
                    Command="{x:Static wizard:RadWizardCommands.MoveCurrentToPrevious}"
                    Content="{Binding PreviousButtonContent, Mode=TwoWay}"
                    Visibility="{Binding NavigationButtonsVisibility.PreviousButtonVisibility}" />
                <telerik:RadButton
                    x:Name="PART_Next"
                    MinWidth="{StaticResource WizardButtonMinWidth}"
                    MinHeight="{StaticResource WizardButtonMinHeight}"
                    Margin="2,0"
                    VerticalAlignment="Center"
                    Command="{x:Static wizard:RadWizardCommands.MoveCurrentToNext}"
                    Content="{Binding NextButtonContent, Mode=TwoWay}"
                    Visibility="{Binding NavigationButtonsVisibility.NextButtonVisibility}" />
                <telerik:RadButton
                    x:Name="PART_Finish"
                    MinWidth="{StaticResource WizardButtonMinWidth}"
                    MinHeight="{StaticResource WizardButtonMinHeight}"
                    Margin="2,0"
                    VerticalAlignment="Center"
                    Command="{x:Static wizard:RadWizardCommands.Finish}"
                    Content="{Binding FinishButtonContent, Mode=TwoWay}"
                    Visibility="{Binding NavigationButtonsVisibility.FinishButtonVisibility}" />
                <telerik:RadButton
                    x:Name="PART_Cancel"
                    MinWidth="{StaticResource WizardButtonMinWidth}"
                    MinHeight="{StaticResource WizardButtonMinHeight}"
                    Margin="2,0"
                    VerticalAlignment="Center"
                    Command="{x:Static wizard:RadWizardCommands.Cancel}"
                    Content="{Binding CancelButtonContent, Mode=TwoWay}"
                    Visibility="{Binding NavigationButtonsVisibility.CancelButtonVisibility}" />
            </StackPanel>
        </Grid>
    </DataTemplate>
    <ControlTemplate x:Key="WizardPageTemplate" TargetType="telerikNavigation:WizardPage">
        <Border
            Background="{TemplateBinding Background}"
            BorderBrush="{TemplateBinding BorderBrush}"
            BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Border
                    x:Name="PART_SideHeaderBorder"
                    Grid.RowSpan="2"
                    Width="{Binding SideHeaderWidth, RelativeSource={RelativeSource TemplatedParent}}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{Binding SideHeaderBorderWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Right}"
                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsSideHeaderVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <ContentPresenter
                        x:Name="PART_SideHeaderPresenter"
                        Margin="{TemplateBinding Padding}"
                        Content="{TemplateBinding SideHeader}"
                        ContentTemplate="{TemplateBinding SideHeaderTemplate}"
                        TextBlock.Foreground="{TemplateBinding Foreground}" />
                </Border>
                <Border
                    x:Name="PART_HeaderBorder"
                    Grid.Column="1"
                    Grid.ColumnSpan="1"
                    Height="{Binding HeaderHeight, RelativeSource={RelativeSource TemplatedParent}}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{Binding HeaderBorderWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Bottom}">
                    <Grid Margin="{TemplateBinding Padding}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <TextBlock
                            x:Name="PART_PageTitle"
                            FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                            FontSize="16"
                            FontWeight="Bold"
                            Foreground="{TemplateBinding Foreground}"
                            Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Title}"
                            TextWrapping="Wrap"
                            Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsTitleVisible, Converter={StaticResource BooleanToVisibilityConverter}}" />
                        <ContentPresenter
                            x:Name="PART_HeaderPresenter"
                            Grid.Row="1"
                            Content="{TemplateBinding Header}"
                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                            TextBlock.Foreground="{TemplateBinding Foreground}"
                            Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHeaderVisible, Converter={StaticResource BooleanToVisibilityConverter}}" />
                    </Grid>
                </Border>
                <ContentPresenter
                    x:Name="PART_PagePresenter"
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="{TemplateBinding Padding}"
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}"
                    TextBlock.FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                    TextBlock.FontSize="{telerik1:GreenResource ResourceKey=FontSize}"
                    TextBlock.Foreground="{TemplateBinding Foreground}" />
                <Border
                    x:Name="PART_FooterBorder"
                    Grid.Row="2"
                    Grid.ColumnSpan="2"
                    MinHeight="{StaticResource WizardFooterMinHeight}"
                    Padding="0,1,0,0"
                    Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                    BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                    BorderThickness="0,1,0,0">
                    <ContentControl
                        x:Name="PART_FooterContentControl"
                        HorizontalContentAlignment="Stretch"
                        VerticalContentAlignment="Center"
                        ContentTemplate="{TemplateBinding FooterTemplate}" />
                </Border>
            </Grid>
        </Border>
    </ControlTemplate>
