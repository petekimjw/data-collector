            <Grid x:Name="HeaderRow" UseLayoutRounding="True">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Border
                    x:Name="BorderVisual"
                    Grid.ColumnSpan="5"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                <Path x:Name="arrow" Style="{TemplateBinding ExpanderStyle}" />
                <ContentControl
                    x:Name="Header"
                    Grid.ColumnSpan="4"
                    Margin="{TemplateBinding Padding}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                    FontSize="{TemplateBinding FontSize}"
                    FontWeight="{TemplateBinding FontWeight}"
                    Foreground="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Foreground, Mode=OneWay}" />
            </Grid>
            <Grid
                x:Name="ItemsContainer"
                Grid.Row="1"
                Margin="0,1,0,0"
                Visibility="Collapsed">
                <telerikPrimitives:LayoutTransformControl x:Name="transformationRoot">
                    <ItemsPresenter />
                </telerikPrimitives:LayoutTransformControl>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsExpanded" Value="True">
                <Setter TargetName="arrow" Property="LayoutTransform">
                    <Setter.Value>
                        <TransformGroup>
                            <RotateTransform Angle="180" />
                        </TransformGroup>
                    </Setter.Value>
                </Setter>
                <Setter TargetName="ItemsContainer" Property="Visibility" Value="Visible" />
                <Setter Property="Margin" Value="0" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Header" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}" />
                <Setter TargetName="arrow" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="arrow" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
            </Trigger>
            <Trigger SourceName="HeaderRow" Property="IsMouseOver" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}" />
                <Setter TargetName="arrow" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}" />
                <Setter TargetName="arrow" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}" />
            </Trigger>
            <DataTrigger Binding="{Binding Path=Orientation, RelativeSource={RelativeSource AncestorType=telerikNavigation:RadPanelBar}}" Value="Horizontal">
                <Setter Property="Margin" Value="0,0,0,1" />
                <Setter TargetName="ItemsContainer" Property="Margin" Value="-1,1,0,0" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="PanelBarItemExpanderStyle" TargetType="Path">
