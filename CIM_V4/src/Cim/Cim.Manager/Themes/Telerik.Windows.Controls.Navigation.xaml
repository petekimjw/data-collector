    <Style
        x:Key="LayoutControlTabGroupStyle"
        BasedOn="{StaticResource LayoutControlGroupBaseStyle}"
        TargetType="telerikNavigation:LayoutControlTabGroup">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <layoutControl:LayoutControlPanel HorizontalAlignment="Left" Orientation="Horizontal" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource LayoutControlTabGroupControlTemplate}" />
