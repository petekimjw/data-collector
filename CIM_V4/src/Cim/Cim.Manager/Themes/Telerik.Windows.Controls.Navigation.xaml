                                        Style="{StaticResource ManipulationPaneButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_SelectParent}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadPathButton>
                                    <telerik:RadPathButton
                                        Margin="0,8,0,10"
                                        Command="{x:Static layoutControl:LayoutControlCommands.DeleteSelectedItem}"
                                        PathGeometry="{StaticResource DeleteSelectedItemIcon}"
                                        Style="{StaticResource ManipulationPaneButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=Delete}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadPathButton>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource RadLayoutControlStyle}" TargetType="telerikNavigation:RadLayoutControl" />
