                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="arrow" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="arrow" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="HeaderContent" Property="Opacity" Value="{telerik:GreenResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="arrow" Property="Opacity" Value="{telerik:GreenResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="arrow" Property="Fill" Value="{telerik:GreenResource ResourceKey=MarkerBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
