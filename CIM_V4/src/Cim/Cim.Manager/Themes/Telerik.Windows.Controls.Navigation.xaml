                            <Border
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="1"
                                CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}">
                                <ItemsPresenter Margin="{TemplateBinding Padding}" />
                            </Border>
                        </Grid>
                        <Grid Visibility="{TemplateBinding DragTooltipVisibility}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Border
                                Grid.ColumnSpan="2"
                                Background="{telerik1:GreenResource ResourceKey=AlternativeBrush}"
                                BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                                BorderThickness="1"
                                CornerRadius="0 1 1 1" />
                            <ContentPresenter
                                x:Name="DropImpossibleIconElement"
                                Content="{TemplateBinding DropImpossibleIcon}"
                                ContentTemplate="{TemplateBinding DropImpossibleIconTemplate}"
                                Visibility="Collapsed" />
                            <ContentPresenter
                                x:Name="DropPossibleIconElement"
                                Content="{TemplateBinding DropPossibleIcon}"
                                ContentTemplate="{TemplateBinding DropPossibleIconTemplate}" />
                            <StackPanel
                                Grid.Column="1"
                                VerticalAlignment="Center"
                                Orientation="Horizontal">
                                <ContentControl
                                    VerticalAlignment="Center"
                                    Content="{TemplateBinding DragActionContent}"
                                    ContentTemplate="{TemplateBinding DragActionContentTemplate}"
                                    Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
                                <ContentControl
                                    Margin="0,0,5,0"
                                    VerticalAlignment="Center"
                                    Content="{TemplateBinding DragTooltipContent}"
                                    ContentTemplate="{TemplateBinding DragTooltipContentTemplate}"
                                    Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
                            </StackPanel>
                        </Grid>
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDropPossible" Value="False">
                            <Setter TargetName="DropImpossibleIconElement" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DropPossibleIconElement" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropImpossibleIcon" Value="The icon is assigned as DataTemplate because assigning as Content triggers a bug in Silverlight" />
        <Setter Property="DropImpossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid Margin="5,5,0,5" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsDropPossible, Converter={StaticResource InvertedBooleanToVisibilityConverter}}">
                        <Path
                            Width="10"
                            Height="10"
                            Data="{StaticResource NoneEffect}"
                            Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"
                            Stretch="Fill" />
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropPossibleIcon" Value="The icon is assigned as DataTemplate because assigning as Content triggers a bug in Silverlight" />
        <Setter Property="DropPossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid Margin="5" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsDropPossible, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <Path
                            Width="10"
                            Height="8"
                            Margin="4,2"
                            Data="{StaticResource DropPossibleIcon}"
                            Fill="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                            Stretch="Fill" />
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadTreeViewStyle" TargetType="telerikNavigation:RadTreeView">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="animation:AnimationManager.AnimationSelector">
