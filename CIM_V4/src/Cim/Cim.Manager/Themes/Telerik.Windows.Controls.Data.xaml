                                        Margin="0 0 4 0"
                                        Command="{x:Static propertyGrid:CollectionEditorCommands.MoveCurrentToNext}"
                                        MinWidth="22"
                                        MinHeight="22"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center">
                                    <Path Data="{StaticResource ArrowDown}" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Path.Resources>
                                            <Style TargetType="Path">
                                                <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=StrongBrush}"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsEnabled, ElementName=moveDownButton}" Value="False">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding IsFocused, ElementName=moveDownButton}" Value="True">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding IsMouseOver, ElementName=moveDownButton}" Value="True">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding IsPressed, ElementName=moveDownButton}" Value="True">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Resources>
                                    </Path>
                                </telerik:RadButton>
                                <telerik:RadButton x:Name="moveUpButton"
                                        IsEnabled="{TemplateBinding IsEnabled}"
                                        telerik:TouchManager.TouchMode="None"
                                        Command="{x:Static propertyGrid:CollectionEditorCommands.MoveCurrentToPrevious}"
                                        MinWidth="22"
                                        MinHeight="22"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Margin="0 0 4 0">
                                    <Path Data="{StaticResource ArrowUp}" VerticalAlignment="Center" HorizontalAlignment="Center">
                                        <Path.Resources>
                                            <Style TargetType="Path">
                                                <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=StrongBrush}"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsEnabled, ElementName=moveUpButton}" Value="False">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding IsFocused, ElementName=moveUpButton}" Value="True">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding IsMouseOver, ElementName=moveUpButton}" Value="True">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding IsPressed, ElementName=moveUpButton}" Value="True">
                                                        <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Resources>
                                    </Path>
                                </telerik:RadButton>
                            </StackPanel>
                        </Grid>
                    </Border>
                    <ListBox x:Name="PART_CollectionListBox"
                            Grid.Row="1"
                            ItemTemplate="{TemplateBinding ItemTemplate}"
                            IsEnabled="{TemplateBinding IsEnabled}"
                            BorderBrush="{x:Null}"
                            BorderThickness="0"
                            ItemsSource="{TemplateBinding CollectionView}"
                            SelectedItem="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=CurrentItem, Mode=TwoWay}"/>
                </Grid>
                <Grid Grid.Row="1" Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Border BorderThickness="1 0 0 1" MinHeight="30" BorderBrush="{TemplateBinding BorderBrush}">
                        <TextBlock telerik:LocalizationManager.ResourceKey="CollectionEditorProperties" VerticalAlignment="Center" Margin="{TemplateBinding Padding}"/>
                    </Border>
                    <controls:RadPropertyGrid
                            Grid.Row="1"
                            Margin="1 0 0 0"
                            IsReadOnly="{TemplateBinding IsReadOnly}"
                            IsEnabled="{TemplateBinding IsEnabled}"
                            Item="{TemplateBinding CurrentItem}"
                            LabelColumnWidth="100"
                            SortAndGroupButtonsVisibility="Collapsed"
                            SearchBoxVisibility="Collapsed"
                            DescriptionPanelVisibility="Collapsed"/>
                </Grid>
                <Border x:Name="FooterPanel_Background" BorderBrush="{TemplateBinding BorderBrush}" Grid.Row="2" BorderThickness="0 1 0 0" Grid.ColumnSpan="2">
                    <Grid>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="0 4">
                            <telerik:RadButton
                                    IsEnabled="{TemplateBinding IsEnabled}"
                                    telerik:TouchManager.TouchMode="None"
                                    MinWidth="50"
                                    telerik:LocalizationManager.ResourceKey="CollectionEditorAdd"
                                    Command="{x:Static propertyGrid:CollectionEditorCommands.AddNew}"
                                    Margin="4 0"/>
                            <telerik:RadButton
                                    IsEnabled="{TemplateBinding IsEnabled}"
                                    telerik:TouchManager.TouchMode="None"
                                    MinWidth="50"
                                    telerik:LocalizationManager.ResourceKey="CollectionEditorRemove"
                                    Command="{x:Static propertyGrid:CollectionEditorCommands.Delete}"/>
                        </StackPanel>
                        <Thumb x:Name="PART_ResizeThumb" Style="{StaticResource CollectionEditorGripperStyle}" Visibility="{TemplateBinding ResizeGripperVisibility}"/>
                    </Grid>
                </Border>
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="CollectionEditorStyle" TargetType="propertyGrid:CollectionEditor">
        <Setter Property="Template" Value="{StaticResource CollectionEditor_Template}"/>
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="4 2"/>
        <Setter Property="Width" Value="495"/>
        <Setter Property="Height" Value="360"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>
    <Style TargetType="propertyGrid:CollectionEditor" BasedOn="{StaticResource CollectionEditorStyle}"/>
    <ControlTemplate x:Key="CollectionEditorPicker_Template" TargetType="propertyGrid:CollectionEditorPicker">
        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock x:Name="PART_CollectionEditorTextBlock"
                        Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Source}"
                        VerticalAlignment="Center"
                        Margin="{TemplateBinding Padding}"/>
                <telerik:RadDropDownButton
                        IsEnabled="{TemplateBinding IsEnabled}"
                        Grid.Column="1"
                        HorizontalContentAlignment="Left"
                        Margin="0 -1 -1 -1"
                        Padding="0"
                        PopupPlacementTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                    <telerik:RadDropDownButton.DropDownContent>
                        <propertyGrid:CollectionEditor x:Name="PART_CollectionEditor"
                                Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=Source}"
                                Header="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=Header}"
                                ResizeGripperVisibility="Visible"
                                IsReadOnly="{TemplateBinding IsReadOnly}"
                                Margin="-1"/>
                    </telerik:RadDropDownButton.DropDownContent>
                </telerik:RadDropDownButton>
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="CollectionEditorPickerStyle" TargetType="propertyGrid:CollectionEditorPicker">
        <Setter Property="Template" Value="{StaticResource CollectionEditorPicker_Template}"/>
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="MinHeight" Value="22"/>
        <Setter Property="Padding" Value="4 2"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource ValidationTooltipTemplate}"/>
    </Style>
    <Style TargetType="propertyGrid:CollectionEditorPicker" BasedOn="{StaticResource CollectionEditorPickerStyle}"/>
    <ControlTemplate x:Key="FlagEnumEditorNonEditableTemplate" TargetType="propertyGrid:FlagEnumEditor">
        <telerik:RadDropDownButton x:Name="PART_EnumValuesDropDownButton"
                HorizontalAlignment="Stretch"
                HorizontalContentAlignment="Stretch"
                IsTabStop="False"
                ClickMode="Press"
                Padding="0"
                Focusable="False"
                TabIndex="1"
                VerticalAlignment="Stretch"
                VerticalContentAlignment="Center">
            <telerik:RadDropDownButton.DropDownContent>
                <ScrollViewer ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" BorderThickness="0" Padding="2">
                    <ItemsControl ItemsSource="{Binding EnumItemsSource, RelativeSource={RelativeSource TemplatedParent}}" BorderThickness="0">
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <CheckBox IsChecked="{Binding IsSelected, Mode=TwoWay}" Content="{Binding Value}" Margin="2 2 0 0" VerticalContentAlignment="Center"/>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </ScrollViewer>
            </telerik:RadDropDownButton.DropDownContent>
            <TextBlock x:Name="FlagEnumEditor_InputTextBlock"
                    Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}"
                    Margin="{TemplateBinding Padding}"
                    Text="{Binding Value, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, ValidatesOnExceptions=True, ValidatesOnDataErrors=True}">
                <TextBlock.Resources>
                    <Style TargetType="TextBlock">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsPressed, ElementName=PART_EnumValuesDropDownButton}" Value="True">
                                <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </TextBlock.Resources>
            </TextBlock>
        </telerik:RadDropDownButton>
    </ControlTemplate>
    <Style x:Key="FlagEnumEditorStyle" TargetType="propertyGrid:FlagEnumEditor">
        <Setter Property="Template" Value="{StaticResource FlagEnumEditorNonEditableTemplate}"/>
        <Setter Property="MinHeight" Value="22"/>
        <Setter Property="Padding" Value="4 2"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource ValidationTooltipTemplate}"/>
    </Style>
    <Style TargetType="propertyGrid:FlagEnumEditor" BasedOn="{StaticResource FlagEnumEditorStyle}"/>
</ResourceDictionary>
