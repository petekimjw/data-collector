                </Border>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="ButtonBackground" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter Property="BorderBrush" TargetName="ButtonBackground" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter Property="Fill" TargetName="DropDownIndicator" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" TargetName="ButtonBackground" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter Property="BorderBrush" TargetName="ButtonBackground" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter Property="Fill" TargetName="DropDownIndicator" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False"/>
            <Trigger Property="IsOpen" Value="True">
                <Setter TargetName="isOpenIndicator" Property="Visibility" Value="Visible"/>
                <Setter TargetName="DropDownIndicator" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
