                <Setter TargetName="textRow2" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="DropDownIndicator" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
