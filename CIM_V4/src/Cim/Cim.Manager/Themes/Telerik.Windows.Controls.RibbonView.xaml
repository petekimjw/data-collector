<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        xmlns:telerikPrimitives="clr-namespace:Telerik.Windows.Controls.Primitives;assembly=Telerik.Windows.Controls"
        xmlns:telerikNavigation="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Navigation"
        xmlns:telerikToolBar="clr-namespace:Telerik.Windows.Controls.ToolBar;assembly=Telerik.Windows.Controls.Navigation"
        xmlns:telerikNavigationPrimitives="clr-namespace:Telerik.Windows.Controls.Primitives;assembly=Telerik.Windows.Controls.Navigation"
        xmlns:telerikRibbonView="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.RibbonView"
        xmlns:telerikKeyTips="clr-namespace:Telerik.Windows.Controls.RibbonView.KeyTips;assembly=Telerik.Windows.Controls.RibbonView"
        xmlns:telerikRibbonViewPrimitives="clr-namespace:Telerik.Windows.Controls.RibbonView.Primitives;assembly=Telerik.Windows.Controls.RibbonView"
        xmlns:c="http://schemas.telerik.com/2008/xaml/compile"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="c"
        xmlns:telerik1="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
        xmlns:telerikRibbonView1="clr-namespace:Telerik.Windows.Controls.RibbonView;assembly=Telerik.Windows.Controls.RibbonView"
        xmlns:shell="clr-namespace:Telerik.Windows.Controls.RibbonView.Shell;assembly=Telerik.Windows.Controls.RibbonView"
        xmlns:localization="clr-namespace:Telerik.Windows.Controls.Localization;assembly=Telerik.Windows.Controls">
    <ResourceDictionary.MergedDictionaries>
        <telerik:GreenResourceDictionary/>
    </ResourceDictionary.MergedDictionaries>
    <telerik1:LocalizationManager x:Key="localizationManagerProvider"/>
    <localization:CultureToLocalizationStringConverter x:Key="cultureToStringConverter"/>
    <Geometry x:Key="ArrowLeft">M0,3 L4,0 L4,6z</Geometry>
    <Geometry x:Key="ArrowRight">M0,0 L4,3 L0,6z</Geometry>
    <Geometry x:Key="ArrowDown">M0,0 L6,0 L3,4z</Geometry>
    <Geometry x:Key="ArrowUp">M0,4 L6,4 L3,0z</Geometry>
    <Geometry x:Key="ThinArrowTop8x5">M0,4L4,0L8,4L8,5L7,5L4,2L1,5L0,5z</Geometry>
    <Geometry x:Key="DropDownArrow">M0,0 6,0 6,1 0,1z M0,2 6,2 3,6z</Geometry>
    <Geometry x:Key="PointerLeft18x14">M6,0L9,0 4,6 18,6 18,8 4,8 9,14 6,14 0,7z</Geometry>
    <Geometry x:Key="ChromeArrow">M3,3L4,3L4,4L6,4L6,3L7,3L7,7L3,7L3,6L4,6L4,4L3,4zM0,0L6,0L6,1L1,1L1,6L0,6z</Geometry>
    <Geometry x:Key="MinimizeIcon">M0,0 L10,0 L10,2 L0,2 z</Geometry>
    <Geometry x:Key="RestoreIcon">M1,5 L1,9 L7,9 L7,5 z M3,2 L3,3 L8,3 L8,6 L9,6 L9,2 z M2,0 L10,0 L10,7 L8,7 L8,10 L0,10 L0,3 L2,3 z</Geometry>
    <Geometry x:Key="MaximizeIcon">M0,0 L10,0 10,10 0,10 z M1,3 L9,3 9,9 1,9 z</Geometry>
    <Geometry x:Key="CloseIcon10x10">M1,0 L5,4 L9,0 L10,1 L6,5 L10,9 L9,10 L5,6 L1,10 L0,9 L4,5 L0,1 z</Geometry>
    <telerik:BooleanToVisibilityConverter x:Key="RibbonBooleanToVisibilityConverter"/>
    <telerik:InvertedBooleanToVisibilityConverter x:Key="InvertBoolToVisConverter"/>
    <telerikRibbonView:BooleanToWindowStateConverter x:Key="BooleanToWindowStateConverter"/>
    <telerikRibbonView1:BooleanToRibbonLayoutConverter x:Key="BooleanToRibbonLayoutConverter"/>
    <telerikRibbonView1:VisibilityToRibbonLayoutConverter x:Key="VisibilityToRibbonLayoutConverter"/>
    <telerikRibbonView:IconConverter x:Key="IconConverter"/>
    <Style x:Key="ToolTipStyle" TargetType="ToolTip">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="4 3"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToolTip">
                    <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <ContentPresenter Margin="{TemplateBinding Padding}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="ToolTip" BasedOn="{StaticResource ToolTipStyle}"/>
    <ControlTemplate x:Key="RadRibbonButtonTemplate" TargetType="telerikRibbonView:RadRibbonButton">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused"/>
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="KeyboardNavigationVisual"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="ButtonBackgroundDecorator"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <Border x:Name="KeyboardNavigationVisual"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Opacity="0"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    Background="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    BorderBrush="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
            <Grid
                    Margin="{TemplateBinding Padding}"
                    x:Name="ContentGrid"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Image Name="Image" VerticalAlignment="Top"/>
                <StackPanel x:Name="FirstRowContainer" Grid.Row="1">
                    <ContentPresenter x:Name="ContentPresenter"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            RecognizesAccessKey="True"
                            ContentStringFormat="{TemplateBinding ContentStringFormat}"
                            TextBlock.Foreground="{TemplateBinding Foreground}"/>
                    <TextBlock Name="textRow1" Text="{TemplateBinding TextRow1}" TextAlignment="Center" Foreground="{TemplateBinding Foreground}"/>
                </StackPanel>
                <TextBlock
                        Name="textRow2"
                        Grid.Row="2"
                        VerticalAlignment="Top"
                        Margin="0 -3 0 0"
                        Text="{TemplateBinding TextRow2}"
                        TextAlignment="Center"
                        Foreground="{TemplateBinding Foreground}"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Image" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow2" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="ContentPresenter" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter Property="TextElement.Foreground" TargetName="ContentPresenter" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="textRow1" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow2" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter TargetName="ButtonBackgroundDecorator" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="ButtonBackgroundDecorator" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow2" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="TextElement.Foreground" TargetName="ContentPresenter" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="ButtonBackgroundDecorator" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter TargetName="ButtonBackgroundDecorator" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow2" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="TextElement.Foreground" TargetName="ContentPresenter" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadRibbonButtonStyle" TargetType="telerikRibbonView:RadRibbonButton">
        <Setter Property="Template" Value="{StaticResource RadRibbonButtonTemplate}"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="MinHeight" Value="22"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="CornerRadius" Value="{telerik1:GreenResource ResourceKey=CornerRadius}"/>
    </Style>
    <Style TargetType="telerikRibbonView:RadRibbonButton" BasedOn="{StaticResource RadRibbonButtonStyle}"/>
    <Style TargetType="telerik:RadToggleButton" x:Key="ToggleButtonStyle">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="BorderBrush" Value="{Binding BorderBrush, RelativeSource={RelativeSource TemplatedParent}}"/>
        <Setter Property="BorderThickness" Value="{Binding BorderThickness, RelativeSource={RelativeSource TemplatedParent}}"/>
        <Setter Property="MinWidth" Value="36"/>
        <Setter Property="MinHeight" Value="24"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Grid Background="Transparent">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.ColumnSpan="2"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                CornerRadius="{TemplateBinding CornerRadius}"
                                BorderThickness="{TemplateBinding BorderThickness}"/>
                        <Border Grid.Column="1">
                            <Path x:Name="Path"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Right"
                                    Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                                    Data="{StaticResource ArrowDown}"
                                    Margin="5 0"/>
                        </Border>
                        <ContentPresenter x:Name="Content" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MainBrush}"/>
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
                            <Setter TargetName="Path" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="EditableRibbonComboBox" TargetType="telerikRibbonView:RadRibbonComboBox">
        <Grid x:Name="ContentPresenterBorder" Background="Transparent">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused"/>
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="KeyboardNavigationVisual"/>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_DropDownButton" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="Transparent"/>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="OuterBorderVisual"
                    Grid.ColumnSpan="2"
                    IsHitTestVisible="False"
                    Background="{telerik1:GreenResource ResourceKey=MainBrush}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"/>
            <Border x:Name="BorderVisual"
                    Grid.ColumnSpan="2"
                    IsHitTestVisible="False"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"/>
            <TextBox x:Name="PART_EditableTextBox"
                    BorderThickness="0"
                    BorderBrush="Transparent"
                    Padding="{TemplateBinding Padding}"
                    FontSize="{TemplateBinding FontSize}"
                    Foreground="{TemplateBinding Foreground}"
                    IsReadOnly="{TemplateBinding IsReadOnly}"
                    Background="Transparent"
                    VerticalAlignment="Center"
                    HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}">
                <TextBox.Template>
                    <ControlTemplate TargetType="TextBox">
                        <Border Background="{TemplateBinding Background}" CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadiusLeft}">
                            <ScrollViewer
                                    Padding="{TemplateBinding Padding}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Background="{TemplateBinding Background}"
                                    IsTabStop="False"
                                    VerticalScrollBarVisibility="Auto"
                                    Name="PART_ContentHost"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                    </ControlTemplate>
                </TextBox.Template>
            </TextBox>
            <Border x:Name="KeyboardNavigationVisual"
                    Opacity="0"
                    Grid.Column="1"
                    Background="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadiusRight}"/>
            <telerik:RadToggleButton x:Name="PART_DropDownButton"
                    Grid.Column="1"
                    Style="{StaticResource ToggleButtonStyle}"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadiusRight}"
                    IsTabStop="False"
                    IsChecked="{TemplateBinding IsDropDownOpen}"
                    IsEnabled="{TemplateBinding IsEnabled}"
                    MinWidth="15"/>
            <TextBlock x:Name="Watermark"
                    Text="{TemplateBinding EmptyText}"
                    Opacity="0.5"
                    IsHitTestVisible="False"
                    VerticalAlignment="Center"
                    HorizontalAlignment="Left"
                    Margin="5 0 0 0"
                    Visibility="Collapsed"/>
            <Popup x:Name="PART_Popup">
                <Grid x:Name="PopupRoot">
                    <Border
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            MinWidth="{TemplateBinding MinDropDownWidth}"
                            MaxHeight="{TemplateBinding MaxDropDownHeight}"
                            Margin="0 -1 0 0"
                            Background="{telerik1:GreenResource ResourceKey=AlternativeBrush}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <telerik:RadButton x:Name="PART_ClearButton"
                                    Margin="-1 -1 -1 0"
                                    CornerRadius="0"
                                    Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
                                    Content="{TemplateBinding ClearSelectionButtonContent}"/>
                            <ScrollViewer x:Name="PART_ScrollViewer"
                                    Grid.Row="1"
                                    Padding="0 1 0 0"
                                    BorderThickness="0"
                                    VerticalScrollBarVisibility="Auto"
                                    Foreground="{TemplateBinding Foreground}"
                                    CanContentScroll="True">
                                <ItemsPresenter/>
                            </ScrollViewer>
                        </Grid>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="PART_EditableTextBox" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="Watermark" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="BorderVisual" Property="Opacity" Value="0"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="PART_DropDownButton" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
            </Trigger>
            <Trigger Property="IsWatermarkVisible" Value="True">
                <Setter TargetName="Watermark" Property="Visibility" Value="Visible"/>
            </Trigger>
            <DataTrigger Binding="{Binding ElementName=PART_DropDownButton, Path=IsPressed}" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
            </DataTrigger>
            <Trigger Property="IsDropDownOpen" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                <Setter TargetName="PART_DropDownButton" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="NonEditableRibbonComboBox" TargetType="telerikRibbonView:RadRibbonComboBox">
        <Grid x:Name="ContentPresenterBorder" Background="Transparent">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused"/>
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="KeyboardNavigationVisual"/>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_DropDownButton" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="Transparent"/>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="OuterBorderVisual" IsHitTestVisible="False" Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
            <Border x:Name="KeyboardNavigationVisual"
                    Opacity="0"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    Background="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"/>
            <telerik:RadToggleButton x:Name="PART_DropDownButton"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                    Style="{StaticResource ToggleButtonStyle}"
                    ClickMode="Release"
                    Background="{TemplateBinding Background}"
                    IsChecked="{TemplateBinding IsDropDownOpen}"
                    IsEnabled="{TemplateBinding IsEnabled}"
                    IsTabStop="False">
                <ContentPresenter x:Name="ContentPresenterElement"
                        Margin="{TemplateBinding Padding}"
                        IsHitTestVisible="False"
                        Content="{TemplateBinding SelectionBoxItem}"
                        ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
            </telerik:RadToggleButton>
            <Popup x:Name="PART_Popup">
                <Grid x:Name="PopupRoot">
                    <Border
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            MinWidth="{TemplateBinding MinDropDownWidth}"
                            MaxHeight="{TemplateBinding MaxDropDownHeight}"
                            Margin="0 -1 0 0"
                            Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <telerik:RadButton x:Name="PART_ClearButton" Margin="-1 -1 -1 0" Visibility="{TemplateBinding ClearSelectionButtonVisibility}" Content="{TemplateBinding ClearSelectionButtonContent}"/>
                            <ScrollViewer x:Name="PART_ScrollViewer"
                                    Grid.Row="1"
                                    Foreground="{TemplateBinding Foreground}"
                                    Padding="0 1"
                                    BorderThickness="0"
                                    VerticalScrollBarVisibility="Auto"
                                    Margin="0"
                                    CanContentScroll="True">
                                <ItemsPresenter/>
                            </ScrollViewer>
                        </Grid>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ContentPresenterElement" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="PART_DropDownButton" Property="Background" Value="{telerik1:GreenResource ResourceKey=MainBrush}"/>
            </Trigger>
            <DataTrigger Binding="{Binding ElementName=PART_DropDownButton, Path=IsPressed}" Value="True">
                <Setter TargetName="PART_DropDownButton" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter TargetName="PART_DropDownButton" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
            </DataTrigger>
            <Trigger Property="IsDropDownOpen" Value="True">
                <Setter TargetName="PART_DropDownButton" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadRibbonComboBoxStyle" TargetType="telerikRibbonView:RadRibbonComboBox">
        <Setter Property="Template" Value="{StaticResource EditableRibbonComboBox}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="Padding" Value="4 2"/>
        <Setter Property="MinHeight" Value="24"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="IsEditable" Value="True"/>
        <Setter Property="Margin" Value="0 0 1 0"/>
        <Setter Property="ClearSelectionButtonContent" Value="Clear"/>
        <Setter Property="EditableTemplate" Value="{StaticResource EditableRibbonComboBox}"/>
        <Setter Property="NonEditableTemplate" Value="{StaticResource NonEditableRibbonComboBox}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <ControlTemplate x:Key="RibbonComboBoxItemTemplate" TargetType="telerikRibbonView:RadRibbonComboBoxItem">
        <Border x:Name="BorderVisual"
                BorderThickness="{TemplateBinding BorderThickness}"
                BorderBrush="{TemplateBinding BorderBrush}"
                Background="{TemplateBinding Background}"
                CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}">
            <ContentPresenter x:Name="Content" Margin="{TemplateBinding Padding}" VerticalAlignment="Center"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="IsHighlighted" Value="True">
