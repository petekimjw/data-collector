                        Height="32">
                    <Image VerticalAlignment="Center" HorizontalAlignment="Center" Width="16" Height="16" Source="{TemplateBinding SmallImage}" Stretch="None"/>
                </Border>
                <TextBlock Name="textRow1" Grid.Row="1" Text="{TemplateBinding TextRow1}" TextAlignment="Center" Foreground="{TemplateBinding Foreground}"/>
                <StackPanel Name="DropDownPanel" Grid.Row="2" HorizontalAlignment="Center" Orientation="Horizontal">
                    <TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" Foreground="{TemplateBinding Foreground}"/>
                    <Path
                            Name="DropDownIndicator"
                            IsHitTestVisible="False"
                            SnapsToDevicePixels="True"
                            Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            Data="{StaticResource ArrowDown}"
                            Margin="2"
                            VerticalAlignment="Top"
                            HorizontalAlignment="Center"/>
                </StackPanel>
            </Grid>
            <Popup x:Name="DropDownPopup"
                    AllowsTransparency="True"
                    StaysOpen="False"
                    Focusable="False"
                    VerticalOffset="2"
                    Placement="Bottom"
                    IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
                <Border x:Name="DropDownPopupBorder"
                        Padding="0"
                        Margin="0"
                        MinWidth="3"
                        MinHeight="3"
                        Width="{TemplateBinding DropDownWidth}"
                        Height="{TemplateBinding DropDownHeight}"
                        MaxWidth="{TemplateBinding DropDownMaxWidth}"
                        MaxHeight="{TemplateBinding DropDownMaxHeight}"
                        Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                        BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                        BorderThickness="1 1 0 1">
                    <ContentControl x:Name="DropDownPopupContent"
                            Content="{TemplateBinding DropDownContent}"
                            ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            VerticalContentAlignment="Stretch"
                            DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"
                            FocusVisualStyle="{x:Null}"/>
                </Border>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow2" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow2" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="DropDownIndicator" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow2" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="ContentPresenter" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow1" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow2" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="True">
                <Setter TargetName="DropDownIndicator" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" TargetName="textRow2" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="DropDownButtonPosition" Value="Right">
                <Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2"/>
                <Setter TargetName="DropDownPanel" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0"/>
                <Setter TargetName="DropDownPanel" Property="Margin" Value="2 0 0 0"/>
                <Setter TargetName="textRow1" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="Image" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="ContentPresenter" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                <Setter TargetName="DropDownIndicator" Property="VerticalAlignment" Value="Center"/>
                <Setter TargetName="ContentGrid" Property="Margin" Value="4 0"/>
                <Setter TargetName="Image" Property="Margin" Value="0"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
