                <telerikRibbonView:CustomContainerStyleSelector>
                    <telerikRibbonView:CustomContainerStyle TypeName="RadRibbonButton" ContainerStyle="{StaticResource RadRibbonButtonStyle}"/>
                    <telerikRibbonView:CustomContainerStyle TypeName="RadRibbonToggleButton" ContainerStyle="{StaticResource RadRibbonToggleButtonStyle}"/>
                    <telerikRibbonView:CustomContainerStyle TypeName="RadRibbonRadioButton" ContainerStyle="{StaticResource RadRibbonRadioButtonStyle}"/>
                    <telerikRibbonView:CustomContainerStyle TypeName="RadRibbonDropDownButton" ContainerStyle="{StaticResource RadRibbonDropDownButtonStyle}"/>
                    <telerikRibbonView:CustomContainerStyle TypeName="RadRibbonSplitButton" ContainerStyle="{StaticResource RadRibbonSplitButtonStyle}"/>
                </telerikRibbonView:CustomContainerStyleSelector>
